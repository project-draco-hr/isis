{
  try {
    return addDays(newSimpleDateFormatUsingDateTimePattern().parse(value),0 - adjustBy);
  }
 catch (  ParseException e) {
    try {
      return addDays(newSimpleDateFormatUsingDatePattern().parse(value),0 - adjustBy);
    }
 catch (    ParseException ex) {
      return null;
    }
  }
}
