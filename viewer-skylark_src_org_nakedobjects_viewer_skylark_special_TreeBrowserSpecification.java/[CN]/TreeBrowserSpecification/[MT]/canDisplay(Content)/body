{
  if (content.isObject()) {
    NakedObject object=(NakedObject)content.getNaked();
    NakedObjectField[] fields=content.getSpecification().getVisibleFields(object,ClientSession.getSession());
    for (int i=0; i < fields.length; i++) {
      if (fields[i].isCollection()) {
        return true;
      }
    }
  }
  return false;
}
