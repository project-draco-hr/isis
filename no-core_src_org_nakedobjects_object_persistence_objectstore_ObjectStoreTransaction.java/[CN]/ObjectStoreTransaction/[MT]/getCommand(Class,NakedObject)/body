{
  for (Enumeration e=commands.elements(); e.hasMoreElements(); ) {
    PersistenceCommand command=(PersistenceCommand)e.nextElement();
    if (command.onObject().equals(onObject)) {
      if (commandClass.isAssignableFrom(command.getClass())) {
        return command;
      }
    }
  }
  return null;
}
