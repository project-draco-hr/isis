{
  final List<ToDoItem> items;
  if (false) {
    items=allMatches(ToDoItem.class,Predicates.and(ToDoItem.Predicates.thoseOwnedBy(currentUserName()),ToDoItem.Predicates.thoseNotYetComplete()));
  }
 else {
    items=allMatches(new QueryDefault<ToDoItem>(ToDoItem.class,"todo_notYetComplete","ownedBy",currentUserName()));
  }
  return items;
}
