{
  if (false) {
    return allMatches(ToDoItem.class,Predicates.and(ToDoItem.Predicates.thoseOwnedBy(currentUserName()),ToDoItem.Predicates.thoseWithSimilarDescription(description)));
  }
 else {
    return allMatches(new QueryDefault<ToDoItem>(ToDoItem.class,"todo_autoComplete","ownedBy",currentUserName(),"description",description));
  }
}
