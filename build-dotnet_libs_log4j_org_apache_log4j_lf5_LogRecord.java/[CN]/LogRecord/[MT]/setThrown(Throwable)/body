{
  if (thrown == null) {
    return;
  }
  _thrown=thrown;
  StringWriter sw=new StringWriter();
  PrintWriter out=new PrintWriter(sw);
  thrown.printStackTrace(out);
  out.flush();
  _thrownStackTrace=sw.toString();
  try {
    out.close();
    sw.close();
  }
 catch (  IOException e) {
  }
  out=null;
  sw=null;
}
