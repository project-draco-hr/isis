{
  int noInstances=0;
  Enumeration e=elements(cls);
  while (e.hasMoreElements()) {
    NakedObject data=(NakedObject)e.nextElement();
    if (cls.equals(data.getSpecification())) {
      noInstances++;
    }
  }
  LOG.debug("numberOfInstances of " + cls + " = "+ noInstances);
  return noInstances;
}
