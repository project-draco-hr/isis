{
  Vector v=getInstances(cls,false);
  int i=0;
  String match=pattern.toLowerCase();
  while (i < v.size()) {
    NakedObject element=(NakedObject)v.elementAt(i);
    if (element.title().toString().toLowerCase().indexOf(match) == -1) {
      v.removeElementAt(i);
    }
 else {
      i++;
    }
  }
  return v;
}
