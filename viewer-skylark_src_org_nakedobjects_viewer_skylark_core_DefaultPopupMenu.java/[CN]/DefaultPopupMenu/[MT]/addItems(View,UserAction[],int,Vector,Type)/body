{
  int initialSize=list.size();
  for (int i=0; i < len; i++) {
    if (options[i].getType() == type) {
      if (initialSize > 0 && list.size() == initialSize) {
        list.addElement(Item.createDivider());
      }
      list.addElement(Item.createOption(options[i],null,target,getLocation()));
    }
  }
}
