{
  loader.addSpec(new DummyNakedObjectSpecification());
  loader.addSpec(new DummyNakedObjectSpecification());
  assertNull(objectWithOneToOneAssoications.getReferencedObject());
  personField.setAssociation(new DummyIdentifier(),nakedObject,associatedNakedObject);
