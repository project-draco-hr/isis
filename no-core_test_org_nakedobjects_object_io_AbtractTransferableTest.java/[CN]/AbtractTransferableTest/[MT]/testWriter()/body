{
  TestWriter writer=new TestWriter();
  writer.writeObject(new TransferableTestObject("smith",34));
  writer.close();
  assertEquals(4,writer.data.size());
  assertEquals(TransferableTestObject.class.getName(),writer.data.elementAt(0));
  assertEquals("CLOSE",writer.data.elementAt(3));
  assertTrue(writer.data.contains("smith"));
  assertTrue(writer.data.contains("34"));
}
