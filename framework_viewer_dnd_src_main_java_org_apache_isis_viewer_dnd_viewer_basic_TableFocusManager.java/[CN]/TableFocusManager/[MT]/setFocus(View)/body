{
  if (view == table) {
    return;
  }
  final View[] rows=table.getSubviews();
  for (row=0; row < rows.length; row++) {
    final View[] cells=rows[row].getSubviews();
    for (int j=0; j < cells.length; j++) {
      if (view == cells[j] && cells[j].canFocus()) {
        cells[cell].markDamaged();
        cell=j;
        cells[j].markDamaged();
        return;
      }
    }
  }
}
