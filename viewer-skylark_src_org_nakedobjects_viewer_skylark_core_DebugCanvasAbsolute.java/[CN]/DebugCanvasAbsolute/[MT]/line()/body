{
  RuntimeException e=new RuntimeException();
  StringWriter s;
  PrintWriter p=new PrintWriter(s=new StringWriter());
  e.printStackTrace(p);
  StringTokenizer st=new StringTokenizer(s.toString(),"\n\r");
  st.nextElement();
  st.nextElement();
  st.nextElement();
  String line=st.nextToken();
  return line.substring(line.indexOf('('));
}
