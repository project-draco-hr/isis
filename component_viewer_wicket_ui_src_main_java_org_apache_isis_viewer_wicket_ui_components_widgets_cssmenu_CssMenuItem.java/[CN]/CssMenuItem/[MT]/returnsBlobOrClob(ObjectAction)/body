{
  boolean blobOrClob=false;
  final ObjectSpecification returnType=objectAction.getReturnType();
  if (returnType != null) {
    Class<?> cls=returnType.getCorrespondingClass();
    if (Blob.class.isAssignableFrom(cls) || Clob.class.isAssignableFrom(cls)) {
      blobOrClob=true;
    }
  }
  return blobOrClob;
}
