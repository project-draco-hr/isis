{
  try {
    getPeer().assertIsList();
  }
 catch (  StoryValueException e) {
    return e.getMessage();
  }
  getPeer().getTitleBinding().setHeadColumn(0);
  getPeer().getAliasBinding().setHeadColumn(1);
  if (type != null) {
    getPeer().getTypeBinding().setHeadColumn(2,new StoryCellDefault(type));
  }
  return null;
}
