{
  iswf.beginTran();
  ApplibValuedEntity entity=repo.newEntity();
  entity.setStringProperty("1");
  Date date=new Date();
  entity.setDateProperty(date);
  iswf.commitTran();
  iswf.bounceSystem();
  iswf.beginTran();
  entity=repo.list().get(0);
  assertThat(entity.getDateProperty().dateValue(),is(date.dateValue()));
  date=date.add(-1,-1,-1);
  entity.setDateProperty(date);
  iswf.commitTran();
  iswf.bounceSystem();
  iswf.beginTran();
  entity=repo.list().get(0);
  assertThat(entity.getDateProperty().dateValue(),is(date.dateValue()));
  iswf.commitTran();
}
