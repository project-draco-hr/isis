{
  super((identifier.getType() == Identifier.Type.PROPERTY_OR_COLLECTION ? "Field" : "Action") + " '" + identifier.getMemberName()+ "' in "+ identifier.getClassNaturalName()+ " is not "+ (isVisibleAndUsabable ? "visible/usable " : "visible")+ " for "+ session.getUserName()+ " "+ session.getRoles());
  this.identifier=identifier;
  this.session=session;
}
