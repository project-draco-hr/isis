{
  BasicConfigurator.configure();
  Logger.getRootLogger().setLevel(Level.OFF);
  om=MockObjectManager.setup();
  new NakedObjectSpecificationLoaderImpl();
  NakedObjectSpecificationImpl.setReflectionFactory(new LocalReflectionFactory());
  NakedObjectContext context=new NakedObjectContext(om);
  Session session=new Session(){
  }
;
  targetObject=new TestObjectExample();
  TestObjectFactory factory=new DefaultTestObjectFactory();
  target=factory.createTestObject(session,PojoAdapter.createAdapter(targetObject));
  singleParameter=factory.createTestObject(session,PojoAdapter.createAdapter(new TestObjectExample()));
  TestObject parameter2=factory.createTestObject(session,PojoAdapter.createAdapter(new TestObjectExample()));
  TestValue valueParameter=factory.createParamerTestValue("a value");
  multipleParameters=new TestNaked[]{singleParameter,parameter2,valueParameter};
  FieldSpecification collectionField=target.getForObject().getSpecification().getField("Collection");
  TestElement element=new TestElement("one");
  elementOne=factory.createTestObject(session,PojoAdapter.createAdapter(element));
  element=new TestElement("two");
  targetObject.getCollection().add(element);
  element=new TestElement("three");
  targetObject.getCollection().add(element);
  elementThree=factory.createTestObject(session,PojoAdapter.createAdapter(element));
}
