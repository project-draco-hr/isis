{
  boolean remoteOverride=getTarget() == Action.REMOTE;
  boolean localOverride=getTarget() == Action.LOCAL;
  if (localOverride) {
    return false;
  }
  if (remoteOverride) {
    return true;
  }
  if (target == null) {
    return false;
  }
  boolean remoteAsPersistent=target.getOid() != null;
  return remoteAsPersistent;
}
