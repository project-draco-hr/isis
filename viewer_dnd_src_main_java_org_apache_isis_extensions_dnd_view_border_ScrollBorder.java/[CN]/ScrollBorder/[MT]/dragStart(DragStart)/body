{
  final int area=adjust(drag);
  dragArea=area;
switch (dragArea) {
case NORTH:
    return topHeader.dragStart(drag);
case WEST:
  return leftHeader.dragStart(drag);
case CENTER:
return wrappedView.dragStart(drag);
case SOUTH:
return dragStartSouth(drag);
case EAST:
return dragStartEast(drag);
default :
return null;
}
}
