{
  StringBuffer buffer=new StringBuffer();
  if (members.length == 0) {
    buffer.append("none");
  }
 else {
    for (int i=0; i < members.length; i++) {
      buffer.append("<a href=\"#" + members[i].getId() + "\">"+ members[i].getName()+ "</a><br>");
    }
  }
  view.appendRow(label,buffer.toString());
}
