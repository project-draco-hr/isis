{
  BasicDBList array=(BasicDBList)instance.get(id);
  final List<StateReader> readers=new ArrayList<StateReader>();
  if (array != null) {
    final int size=array.size();
    for (int i=0; i < size; i++) {
      readers.add(new MongoStateReader((DBObject)array.get(i)));
    }
  }
  return readers;
}
