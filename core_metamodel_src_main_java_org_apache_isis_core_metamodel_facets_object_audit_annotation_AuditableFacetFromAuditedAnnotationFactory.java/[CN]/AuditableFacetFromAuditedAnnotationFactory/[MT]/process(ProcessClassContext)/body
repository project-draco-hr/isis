{
  final Class<?> cls=processClassContext.getCls();
  final Audited annotation=Annotations.getAnnotation(cls,Audited.class);
  if (annotation == null) {
    return;
  }
  FacetUtil.addFacet(new AuditableFacetAuditedAnnotation(processClassContext.getFacetHolder(),Enablement.ifDisabled(annotation.disabled())));
  return;
}
