{
  if (value == null) {
    LOG.debug("ignoring " + key + " as value is null");
    return;
  }
  if (key == null) {
    return;
  }
  if (properties.containsKey(key)) {
    LOG.info("skipping " + key + "="+ value+ " as value already set (with "+ properties.get(key)+ ")");
  }
 else {
    LOG.info("adding " + key + "="+ value);
    properties.put(key,value);
  }
}
