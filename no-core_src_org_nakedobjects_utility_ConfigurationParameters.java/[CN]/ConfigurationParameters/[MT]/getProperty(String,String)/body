{
  String key=fullName(name);
  if (key.indexOf("..") >= 0) {
    throw new NakedObjectRuntimeException("property names should not have '..' within them: " + name);
  }
  String property=p.getProperty(key,defaultValue);
  LOG.debug("property: " + key + " =  <"+ property+ ">");
  return property;
}
