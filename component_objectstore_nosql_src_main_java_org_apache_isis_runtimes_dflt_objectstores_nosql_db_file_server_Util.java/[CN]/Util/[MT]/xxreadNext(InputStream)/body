{
  final byte[] buffer=new byte[100];
  int c;
  int i=0;
  while ((c=input.read()) != ' ' && c != '\n' && c != -1) {
    buffer[i++]=(byte)c;
  }
  if (i == 0) {
    throw new NoSqlStoreException("No data read from " + input);
  }
  final String read=new String(buffer,0,i);
  LOG.debug("read " + read);
  return read;
}
