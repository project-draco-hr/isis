{
  String opt=extractOption();
  int r=0;
  if (opt != null) {
    try {
      r=Integer.parseInt(opt);
      if (r <= 0) {
        LogLog.error("Precision option (" + opt + ") isn't a positive integer.");
        r=0;
      }
    }
 catch (    NumberFormatException e) {
      LogLog.error("Category option \"" + opt + "\" not a decimal integer.",e);
    }
  }
  return r;
}
