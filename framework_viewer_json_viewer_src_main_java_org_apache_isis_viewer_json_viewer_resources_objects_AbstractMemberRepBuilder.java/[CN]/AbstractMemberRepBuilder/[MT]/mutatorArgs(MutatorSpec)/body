{
  if (mutatorSpec.bodyArgs.isNone()) {
    return null;
  }
  if (mutatorSpec.bodyArgs.isOne()) {
    final Representation argRep=new Representation();
    argRep.put("arg",null);
    return argRep;
  }
  throw new UnsupportedOperationException("should be overridden if bodyArgs is not 0 or 1");
}
