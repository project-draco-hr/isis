{
  View r=table.getSubviews()[row];
  View[] cells=r.getSubviews();
  for (int j=cell + 1; j < cells.length; j++) {
    if (cells[j].canFocus()) {
      cell=j;
      setFocus(cells[cell]);
      return;
    }
  }
  row++;
  if (row == table.getSubviews().length) {
    row=0;
  }
  r=table.getSubviews()[row];
  cells=r.getSubviews();
  for (int j=0; j < cells.length; j++) {
    if (cells[j].canFocus()) {
      cell=j;
      setFocus(cells[cell]);
      return;
    }
  }
}
