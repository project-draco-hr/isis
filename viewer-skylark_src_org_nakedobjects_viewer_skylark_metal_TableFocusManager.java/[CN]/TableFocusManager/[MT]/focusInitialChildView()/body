{
  row=cell=-1;
  View[] rows=table.getSubviews();
  if (rows.length > 0) {
    row=0;
    View[] cells=rows[0].getSubviews();
    for (int j=0; j < cells.length; j++) {
      if (cells[j].canFocus()) {
        cells[cell].markDamaged();
        cell=j;
        cells[j].markDamaged();
        return;
      }
    }
  }
}
