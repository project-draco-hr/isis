{
  if (!LOG.isDebugEnabled()) {
    return "";
  }
  final StringBuilder str=new StringBuilder();
  str.append(message);
  str.append(" ");
  str.append(identifier);
  str.append(" on ");
  str.append(target);
  for (int i=0; i < parameters.length; i++) {
    if (i > 0) {
      str.append(',');
    }
    str.append(parameters[i]);
  }
  return str.toString();
}
