{
  view.divider("Objects for " + scope);
  Map<String,Mapping> map=scopedMappings.get(scope);
  Iterator<String> ids=new TreeSet(map.keySet()).iterator();
  if (!ids.hasNext()) {
    view.appendRow("None","");
  }
  while (ids.hasNext()) {
    String key=ids.next();
    view.appendRow(key,map.get(key).debug());
  }
}
