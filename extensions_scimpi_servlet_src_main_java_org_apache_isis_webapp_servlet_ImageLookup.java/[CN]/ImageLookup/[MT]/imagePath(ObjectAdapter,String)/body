{
  String name=object.getIconName();
  String imageName=(String)images.get(name);
  if (imageName != null) {
    return (String)imageName;
  }
 else {
    String imageFile=imageFile(name,contextPath);
    if (imageFile != null) {
      return imageFile;
    }
 else {
      return findImage(object.getSpecification(),contextPath);
    }
  }
}
