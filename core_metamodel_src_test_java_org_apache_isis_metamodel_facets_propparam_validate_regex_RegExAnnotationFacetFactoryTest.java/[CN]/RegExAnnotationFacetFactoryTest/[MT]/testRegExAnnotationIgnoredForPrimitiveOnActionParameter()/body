{
@edu.umd.cs.findbugs.annotations.SuppressWarnings("UMAC_UNCALLABLE_METHOD_OF_ANONYMOUS_CLASS") class Customer {
    @SuppressWarnings("unused") public void someAction(    @RegEx(validation="^A.*",caseSensitive=false) final int foo){
    }
  }
  final Method method=findMethod(Customer.class,"someAction",new Class[]{int.class});
  facetFactory.processParams(method,0,facetHolder);
  assertNull(facetHolder.getFacet(RegExFacet.class));
}
