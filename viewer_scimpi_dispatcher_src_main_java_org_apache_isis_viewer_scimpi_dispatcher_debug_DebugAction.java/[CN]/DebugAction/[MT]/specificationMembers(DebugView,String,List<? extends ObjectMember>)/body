{
  StringBuffer buffer=new StringBuffer();
  if (members.size() == 0) {
    buffer.append("none");
  }
 else {
    for (int i=0; i < members.size(); i++) {
      buffer.append("<a href=\"#" + members.get(i).getId() + "\">"+ members.get(i).getName()+ "</a><br>");
    }
  }
  view.appendRow(label,buffer.toString());
}
