{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  AdapterAndProperty other=(AdapterAndProperty)obj;
  if (objectAdapter == null) {
    if (other.objectAdapter != null)     return false;
  }
 else   if (!objectAdapter.equals(other.objectAdapter))   return false;
  if (property == null) {
    if (other.property != null)     return false;
  }
 else   if (!property.equals(other.property))   return false;
  return true;
}
