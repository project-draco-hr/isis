{
  if (LOG.isDebugEnabled()) {
    LOG.debug("flushTransaction");
  }
  if (getTransaction().getState().mustAbort()) {
    throw getTransaction().getAbortCause();
  }
  if (getTransaction() != null) {
    persistenceSession.objectChangedAllDirty();
    getTransaction().flush();
  }
  return false;
}
