{
  try {
    String encryptionType;
    if (instance.has("_encrypt")) {
      encryptionType=instance.getString("_encrypt");
    }
 else {
      encryptionType="none";
    }
    return encryptionType;
  }
 catch (  final JSONException e) {
    throw new NoSqlStoreException("failed to read field _encrypt",e);
  }
}
