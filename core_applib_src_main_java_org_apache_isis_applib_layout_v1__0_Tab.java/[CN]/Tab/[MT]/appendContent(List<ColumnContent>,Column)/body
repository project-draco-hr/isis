{
  if (column == null) {
    return;
  }
  final List<PropertyGroup> propertyGroups=column.getPropertyGroups();
  if (propertyGroups != null) {
    contents.addAll(propertyGroups);
  }
  final List<Collection> collections=column.getCollections();
  if (collections != null) {
    contents.addAll(collections);
  }
}
