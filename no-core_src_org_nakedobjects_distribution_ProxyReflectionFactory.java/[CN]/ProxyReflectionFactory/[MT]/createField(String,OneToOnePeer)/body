{
  OneToOnePeer oneToOneDelegate=new ProxyOneToOneAssociation(local,connection);
  oneToOneDelegate=new OneToOneHelp(oneToOneDelegate,helpLookup);
  OneToOneAssociation association=new OneToOneAssociation(className,oneToOneDelegate.getName(),oneToOneDelegate.getType(),oneToOneDelegate);
  return association;
}
