{
  out.println("<table width=80% border=0 cellpadding=0>");
  Enumeration e=collection.elements();
  while (e.hasMoreElements()) {
    out.println("<tr><td>");
    NakedObject element=(NakedObject)e.nextElement();
    out.println(objectLink((NakedObject)element,inObject));
    out.println("&nbsp;<button name=remove-element value=" + URLEncoder.encode("xxx",ENCODING) + " type=submit>remove</button>");
    out.println("</td></tr>");
  }
  out.println("<button name=add-element value=xxx" + " type=submit>add</button>");
  out.println("</table>");
}
