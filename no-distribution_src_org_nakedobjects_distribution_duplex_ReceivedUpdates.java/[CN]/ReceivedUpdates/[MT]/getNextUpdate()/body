{
  while (updates.size() == 0 && running) {
    try {
      wait();
    }
 catch (    InterruptedException ignore) {
    }
  }
  if (running) {
    ObjectUpdateMessage update=(ObjectUpdateMessage)updates.elementAt(0);
    updates.removeElementAt(0);
    return update;
  }
 else {
    return null;
  }
}
