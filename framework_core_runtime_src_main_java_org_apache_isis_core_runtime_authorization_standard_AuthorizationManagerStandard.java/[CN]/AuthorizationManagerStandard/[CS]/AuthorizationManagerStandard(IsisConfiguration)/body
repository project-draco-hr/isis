{
  super(configuration);
  authorizor=new Authorizor(){
    @Override public void init(){
    }
    @Override public void shutdown(){
    }
    @Override public boolean isVisibleInRole(    final String user,    final Identifier identifier){
      return true;
    }
    @Override public boolean isUsableInRole(    final String role,    final Identifier identifier){
      return true;
    }
    @Override public boolean isVisibleInAnyRole(    Identifier identifier){
      return true;
    }
    @Override public boolean isUsableInAnyRole(    Identifier identifier){
      return true;
    }
  }
;
}
