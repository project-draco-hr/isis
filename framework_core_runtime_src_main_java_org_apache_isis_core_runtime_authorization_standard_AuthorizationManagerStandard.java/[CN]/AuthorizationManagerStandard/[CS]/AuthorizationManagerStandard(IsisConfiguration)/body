{
  super(configuration);
  authorizor=new Authorizor(){
    @Override public void init(){
    }
    @Override public void shutdown(){
    }
    @Override public boolean isVisibleInRole(    String user,    Identifier identifier){
      return true;
    }
    @Override public boolean isUsableInRole(    String role,    Identifier identifier){
      return true;
    }
  }
;
}
