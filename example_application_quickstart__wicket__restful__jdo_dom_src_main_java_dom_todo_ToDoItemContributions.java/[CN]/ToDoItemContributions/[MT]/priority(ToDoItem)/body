{
  if (toDoItem.isComplete()) {
    return null;
  }
  final List<ToDoItem> sortedNotYetComplete=ORDERING_DUE_BY.compound(ORDERING_DESCRIPTION).sortedCopy(toDoItems.notYetComplete());
  int i=1;
  for (  ToDoItem each : sortedNotYetComplete) {
    if (each == toDoItem) {
      return i;
    }
    i++;
  }
  return null;
}
