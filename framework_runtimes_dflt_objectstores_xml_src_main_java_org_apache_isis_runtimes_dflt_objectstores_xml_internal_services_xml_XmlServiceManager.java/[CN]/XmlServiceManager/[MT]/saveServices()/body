{
  xmlFile.writeXml(SERVICES_FILE_NAME,new ContentWriter(){
    @Override public void write(    final Writer writer) throws IOException {
      final String tag=SERVICES_FILE_NAME;
      writer.append("<" + tag + ">\n");
      for (      final ServiceElement element : services) {
        writer.append("  <service");
        Utils.appendAttribute(writer,"oid",element.oid.enString());
        writer.append("/>\n");
      }
      writer.append("</" + tag + ">\n");
    }
  }
);
}
