{
  boolean hasExtension=false;
  for (  final String extension : EXTENSIONS) {
    hasExtension=hasExtension || name.endsWith(extension);
  }
  final List<String> candidates=new ArrayList<String>();
  if (hasExtension) {
    candidates.add(name);
  }
 else {
    for (    final String extension : EXTENSIONS) {
      candidates.add(name + "." + extension);
    }
  }
  return candidates;
}
