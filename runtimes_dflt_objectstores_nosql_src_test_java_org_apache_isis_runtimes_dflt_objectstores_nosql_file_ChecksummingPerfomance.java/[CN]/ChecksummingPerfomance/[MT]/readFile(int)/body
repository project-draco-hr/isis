{
  BufferedReader reader=new BufferedReader(new InputStreamReader(new FileInputStream("test" + i % 3 + ".data"),Util.ENCODING));
  StringBuffer buf=new StringBuffer();
  String line;
  while ((line=reader.readLine()) != null) {
    buf.append(line);
    buf.append('\n');
  }
  reader.close();
  return buf;
}
