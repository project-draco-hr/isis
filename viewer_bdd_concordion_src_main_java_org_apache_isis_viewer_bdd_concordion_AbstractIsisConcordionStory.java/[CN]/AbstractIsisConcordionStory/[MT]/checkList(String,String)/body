{
  if (executingTable()) {
    if (executingTableHeader()) {
      checkList=new CheckListForConcordion(getStory().getAliasRegistry(),listAlias);
      return checkList.executeHeader(title);
    }
 else {
      return checkList.executeRow(title);
    }
  }
 else {
    checkList=new CheckListForConcordion(getStory().getAliasRegistry(),listAlias);
    checkList.executeHeader(title);
    return checkList.executeRow(title);
  }
}
