{
  ac.first();
  ac.next();
  Enumeration data=ac.displayElements();
  int i=ac.getDisplaySize();
  while (data.hasMoreElements()) {
    Role element=(Role)data.nextElement();
    assertEquals(elements[i++],element);
  }
  ac.next();
  data=ac.displayElements();
  while (data.hasMoreElements()) {
    Role element=(Role)data.nextElement();
    assertEquals(elements[i++],element);
  }
  ac.next();
  data=ac.displayElements();
  while (data.hasMoreElements()) {
    Role element=(Role)data.nextElement();
    assertEquals(elements[i++],element);
  }
}
