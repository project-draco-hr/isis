{
  return Lists.newArrayList(Collections2.filter(fixtureScriptList,new Predicate<FixtureScript>(){
    @Override public boolean apply(    FixtureScript input){
      return contains(input.getFriendlyName()) || contains(input.getLocalName());
    }
    private boolean contains(    String str){
      return str != null && str.contains(arg);
    }
  }
));
}
