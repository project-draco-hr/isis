{
  String result=s;
  for (int i=0; i < specialChars.length; i++) {
    String special=specialChars[i];
    int pos=-1;
    while (true) {
      pos=result.indexOf(special,pos + 1);
      if (pos < 0)       break;
      result=result.substring(0,pos) + escapeString[i] + result.substring(pos + special.length());
    }
  }
  return result;
}
