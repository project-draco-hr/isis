{
  if (newPickup != null) {
    if (getCity() == null || getDropOff() == null) {
      return null;
    }
 else {
      if (!getCity().equals(newPickup.getCity())) {
        return "Location must be in " + getCity();
      }
      if (newPickup.equals(getDropOff())) {
        return "Pick up must differ from the drop off location";
      }
    }
  }
  return null;
}
