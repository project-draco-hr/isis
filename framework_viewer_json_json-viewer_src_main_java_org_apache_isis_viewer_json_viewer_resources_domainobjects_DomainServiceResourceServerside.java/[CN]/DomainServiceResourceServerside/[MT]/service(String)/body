{
  RepresentationType representationType=RepresentationType.DOMAIN_OBJECT;
  init(representationType);
  final ObjectAdapter serviceAdapter=getServiceAdapter(serviceId);
  final RendererFactory factory=rendererFactoryRegistry.find(representationType);
  final DomainObjectReprRenderer renderer=(DomainObjectReprRenderer)factory.newRenderer(getResourceContext(),JsonRepresentation.newMap());
  renderer.usingLinkToBuilder(new DomainServiceLinkToBuilder()).includesSelf().with(serviceAdapter);
  return responseOfOk(Caching.ONE_DAY,renderer).build();
}
