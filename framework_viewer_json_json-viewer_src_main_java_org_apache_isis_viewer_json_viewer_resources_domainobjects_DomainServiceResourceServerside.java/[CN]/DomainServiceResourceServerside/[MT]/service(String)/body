{
  RepresentationType representationType=RepresentationType.DOMAIN_OBJECT;
  init(representationType);
  final ObjectAdapter serviceAdapter=getServiceAdapter(serviceId);
  final RendererFactory factory=rendererFactoryRegistry.find(representationType);
  final DomainObjectReprRenderer renderer=(DomainObjectReprRenderer)factory.newRenderer(getResourceContext(),null,JsonRepresentation.newMap());
  renderer.usingLinkToBuilder(new DomainServiceLinkTo()).includesSelf().with(serviceAdapter);
  return responseOfOk(renderer,Caching.ONE_DAY).build();
}
