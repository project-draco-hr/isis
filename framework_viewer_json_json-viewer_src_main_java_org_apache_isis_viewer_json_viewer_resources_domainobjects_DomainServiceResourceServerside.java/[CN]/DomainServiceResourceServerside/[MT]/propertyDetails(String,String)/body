{
  final ObjectAdapter serviceAdapter=getServiceAdapter(serviceId);
  final OneToOneAssociation property=DomainObjectResourceServerside.getPropertyThatIsVisibleAndUsable(getResourceContext(),serviceAdapter,propertyId,Intent.ACCESS);
  final ObjectPropertyRepBuilder builder=ObjectPropertyRepBuilder.newBuilder(getResourceContext(),serviceAdapter,property);
  return responseOfOk(jsonFrom(builder));
}
