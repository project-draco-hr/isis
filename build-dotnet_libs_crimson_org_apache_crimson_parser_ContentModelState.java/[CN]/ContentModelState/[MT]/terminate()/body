{
switch (model.type) {
case '+':
    if (!sawOne && !((ContentModel)model).empty())     return false;
case '*':
case '?':
  return (next == null) || next.terminate();
case '|':
return model.empty() && (next == null || next.terminate());
case ',':
ContentModel m;
for (m=model; (m != null) && m.empty(); m=m.next) continue;
if (m != null) return false;
return (next == null) || next.terminate();
case 0:
return false;
default :
throw new InternalError();
}
}
