{
  if (!interactionHelper.hasEventBusService()) {
    return null;
  }
  currentInteraction.set(null);
  final PropertyDomainEvent<?,?> event=interactionHelper.postEventForProperty(eventType(),null,AbstractInteractionEvent.Phase.DISABLE,getIdentified(),ic.getTarget(),null,null);
  if (event != null && event.isDisabled()) {
    return event.getDisabledReason();
  }
  return null;
}
