{
  final PropertyInteractionEvent<?,?> existingEvent=currentInteraction.get();
  final PropertyInteractionEvent<?,?> event=interactionHelper.postEventForProperty(eventType(),existingEvent,AbstractInteractionEvent.Phase.DISABLE,ic.getTarget(),getIdentified(),null,null);
  if (event == null || !event.isDisabled()) {
    currentInteraction.set(event);
    return null;
  }
 else {
    currentInteraction.set(null);
    return event.getDisabledReason();
  }
}
