{
  if (!interactionHelper.hasEventBusService()) {
    return null;
  }
  currentInteraction.set(null);
  final PropertyInteractionEvent<?,?> event=interactionHelper.postEventForProperty(eventType(),null,AbstractInteractionEvent.Phase.HIDE,getIdentified(),ic.getTarget(),null,null);
  if (event != null && event.isHidden()) {
    return "Hidden by subscriber";
  }
  return null;
}
