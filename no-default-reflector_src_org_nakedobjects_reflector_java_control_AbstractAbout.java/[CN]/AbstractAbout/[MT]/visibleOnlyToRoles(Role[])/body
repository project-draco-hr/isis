{
  concatDebug("Visible only to roles ");
  boolean validRole=false;
  for (int i=0; i < roles.length; ++i) {
    if (roles[i] == null) {
      continue;
    }
    debug.append(i > 0 ? ", " : "" + roles[i].getName());
    validRole=validRole || (currentUserHasRole(roles[i]));
  }
  if (!validRole) {
    vetoAccess();
  }
}
