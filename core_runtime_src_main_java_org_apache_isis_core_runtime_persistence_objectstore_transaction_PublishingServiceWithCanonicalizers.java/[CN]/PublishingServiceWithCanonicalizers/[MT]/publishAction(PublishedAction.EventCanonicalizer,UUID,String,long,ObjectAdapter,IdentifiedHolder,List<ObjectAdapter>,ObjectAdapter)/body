{
  final PublishedAction.EventCanonicalizer eventCanonicalizerToUse=eventCanonicalizer != null ? eventCanonicalizer : defaultActionEventCanonicalizer;
  final CanonicalEvent canonicalizedEvent=eventCanonicalizerToUse.canonicalizeAction(ObjectAdapterUtils.unwrapObject(targetAdapter),action.getIdentifier(),ObjectAdapterUtils.unwrapObjects(parameterAdapters),ObjectAdapterUtils.unwrapObject(resultAdapter));
  publishingService.publish(guid,currentUser,currentTimestampEpoch,canonicalizedEvent);
}
