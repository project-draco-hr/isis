{
  final FacetHolder facetHolder=processClassContext.getFacetHolder();
  final TranslationService translationService=servicesInjector.lookupService(TranslationService.class);
  final GridService gridService=servicesInjector.lookupService(GridService.class);
  final GridNormalizerService gridNormalizerService=servicesInjector.lookupService(GridNormalizerService.class);
  FacetUtil.addFacet(GridFacetDefault.create(facetHolder,translationService,gridService,gridNormalizerService,getDeploymentCategory()));
}
