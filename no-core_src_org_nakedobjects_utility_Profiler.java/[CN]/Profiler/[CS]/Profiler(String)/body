{
  this.name=name;
synchronized (Profiler.class) {
    id=nextId++;
  }
  Thread t=Thread.currentThread();
  String thread=(String)threads.get(t);
  if (thread != null) {
    this.thread=thread;
  }
 else {
    this.thread="t" + nextThread++;
    threads.put(t,this.thread);
  }
  memory=memory();
}
