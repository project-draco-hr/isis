{
  ObjectAction action=null;
  action=noSpec.getObjectAction(ActionType.USER,actionId);
  if (action != null) {
    return action;
  }
  action=noSpec.getObjectAction(ActionType.EXPLORATION,actionId);
  if (action != null) {
    return action;
  }
  action=noSpec.getObjectAction(ActionType.DEBUG,actionId);
  if (action != null) {
    return action;
  }
  return null;
}
