{
  NakedObject object=(NakedObject)new NakedClassImpl("test naked class");
  Content content=new RootObject(object);
  ViewSpecification specification=null;
  ViewAxis axis=null;
  ClassIcon view=new ClassIcon(content,specification,axis);
  view.setLocation(new Location(100,60));
  view.setSize(view.getRequiredSize());
  workspace.addView(view);
}
