{
  nsSupport.reset();
  if (entityResolver != null) {
    parser.setEntityResolver(entityResolver);
  }
  if (dtdHandler != null) {
    parser.setDTDHandler(dtdHandler);
  }
  if (errorHandler != null) {
    parser.setErrorHandler(errorHandler);
  }
  parser.setDocumentHandler(this);
  locator=null;
}
