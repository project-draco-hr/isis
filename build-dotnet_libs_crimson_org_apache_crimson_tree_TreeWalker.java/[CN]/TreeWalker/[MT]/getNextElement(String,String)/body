{
  for (Node next=getNext(); next != null; next=getNext()) {
    if (next.getNodeType() == Node.ELEMENT_NODE && (nsURI == null || nsURI.equals(next.getNamespaceURI())) && (localName == null || localName.equals(next.getLocalName()))) {
      return (Element)next;
    }
  }
  current=null;
  return null;
}
