{
  try {
    in=new ObjectInputStream(is);
    Request request=(Request)in.readObject();
    LOG.debug("received request " + request);
    return request;
  }
 catch (  ClassNotFoundException e) {
    e.printStackTrace();
    throw new ServerRequestException(e);
  }
catch (  IOException e) {
    e.printStackTrace();
    throw new ServerRequestException(e);
  }
}
