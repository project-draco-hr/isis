{
  if (parameterMap == null) {
    return defaultValue;
  }
  @SuppressWarnings("unchecked") Map<String,String[]> parameters=(Map<String,String[]>)parameterMap;
  final String[] values=parameters.get(getName());
  if (values == null) {
    return defaultValue;
  }
  if (values.length == 1) {
    return getParser().valueOf(urlDecode(values[0]));
  }
  return getParser().valueOf(urlDecode(values));
}
