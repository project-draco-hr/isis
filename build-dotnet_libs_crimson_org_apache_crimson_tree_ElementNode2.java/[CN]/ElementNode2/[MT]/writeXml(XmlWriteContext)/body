{
  Writer out=context.getWriter();
  if (qName == null)   throw new IllegalStateException(getMessage("EN-002"));
  out.write(tagStart,0,1);
  out.write(qName);
  if (attributes != null)   attributes.writeXml(context);
  if (!hasChildNodes())   out.write(tagEnd,0,3);
 else {
    out.write(tagEnd,2,1);
    writeChildrenXml(context);
    out.write(tagStart,0,2);
    out.write(qName);
    out.write(tagEnd,2,1);
  }
}
