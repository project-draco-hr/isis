{
  try {
    ElementNode2 retval=makeClone();
    if (deep) {
      for (int i=0; true; i++) {
        Node node=item(i);
        if (node == null)         break;
        retval.appendChild(node.cloneNode(true));
      }
    }
    return retval;
  }
 catch (  DOMException e) {
    throw new RuntimeException(getMessage("EN-001"));
  }
}
