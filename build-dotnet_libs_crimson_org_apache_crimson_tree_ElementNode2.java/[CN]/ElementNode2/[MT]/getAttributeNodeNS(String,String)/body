{
  if (localName == null) {
    return null;
  }
  if (attributes == null) {
    return null;
  }
  for (int i=0; ; i++) {
    AttributeNode attr=(AttributeNode)attributes.item(i);
    if (attr == null) {
      return null;
    }
    if (localName.equals(attr.getLocalName()) && (attr.getNamespaceURI() == namespaceURI || attr.getNamespaceURI().equals(namespaceURI))) {
      return attr;
    }
  }
}
