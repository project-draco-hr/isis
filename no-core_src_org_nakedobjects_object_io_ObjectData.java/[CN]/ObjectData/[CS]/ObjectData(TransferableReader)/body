{
  super(data);
  int size=data.readInt();
  for (int i=0; i < size; i++) {
    String key=data.readString();
    String type=data.readString();
    if (type.equals("O")) {
      Transferable object=data.readObject();
      fields.put(key,object);
    }
 else {
      String value=data.readString();
      fields.put(key,value);
    }
  }
}
