{
  if (is == null) {
    throw new IllegalArgumentException("InputSource cannot be null");
  }
  Parser parser=this.getParser();
  if (hb != null) {
    parser.setDocumentHandler(hb);
    parser.setEntityResolver(hb);
    parser.setErrorHandler(hb);
    parser.setDTDHandler(hb);
  }
  parser.parse(is);
}
