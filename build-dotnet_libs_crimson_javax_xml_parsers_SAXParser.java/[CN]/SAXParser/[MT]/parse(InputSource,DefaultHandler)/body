{
  if (is == null) {
    throw new IllegalArgumentException("InputSource cannot be null");
  }
  XMLReader reader=this.getXMLReader();
  if (dh != null) {
    reader.setContentHandler(dh);
    reader.setEntityResolver(dh);
    reader.setErrorHandler(dh);
    reader.setDTDHandler(dh);
  }
  reader.parse(is);
}
