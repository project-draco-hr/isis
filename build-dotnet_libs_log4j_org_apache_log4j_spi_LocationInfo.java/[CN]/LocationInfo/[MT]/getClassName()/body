{
  if (fullInfo == null)   return NA;
  if (className == null) {
    int iend=fullInfo.lastIndexOf('(');
    if (iend == -1)     className=NA;
 else {
      iend=fullInfo.lastIndexOf('.',iend);
      int ibegin=0;
      if (inVisualAge) {
        ibegin=fullInfo.lastIndexOf(' ',iend) + 1;
      }
      if (iend == -1)       className=NA;
 else       className=this.fullInfo.substring(ibegin,iend);
    }
  }
  return className;
}
