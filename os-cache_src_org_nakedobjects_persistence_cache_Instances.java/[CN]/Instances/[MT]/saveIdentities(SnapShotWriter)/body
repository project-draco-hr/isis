{
  writer.writeInt(numberInstances());
  Enumeration e=instances();
  int i=0;
  while (e.hasMoreElements()) {
    NakedObject object=(NakedObject)e.nextElement();
    Object oid=object.getOid();
    writer.writeOid(oid);
    LOG.debug("write 1: " + i++ + " "+ specification.getFullName()+ "/"+ oid);
  }
}
