{
  final String href=givenLinkToService();
  final RestfulRequest request=client.createRequest(HttpMethod.GET,href).withArg(QueryParameter.FOLLOW_LINKS,"members[actionId='%s'].actionDetails","list");
  final RestfulResponse<ObjectActionRepresentation> restfulResponse=request.executeT();
  assertThat(restfulResponse.getStatus(),is(HttpStatusCode.OK));
}
