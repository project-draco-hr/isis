{
  if ((offset + 1) >= finish)   return false;
  char c1=buf[offset++];
  char c2=buf[offset];
  if ((c1 >= 0xd800 && c1 < 0xdc00) && (c2 >= 0xdc00 && c2 <= 0xdfff))   return true;
  fatal("P-074",new Object[]{Integer.toHexString(c1 & 0x0ffff),Integer.toHexString(c2 & 0x0ffff)});
  return false;
}
