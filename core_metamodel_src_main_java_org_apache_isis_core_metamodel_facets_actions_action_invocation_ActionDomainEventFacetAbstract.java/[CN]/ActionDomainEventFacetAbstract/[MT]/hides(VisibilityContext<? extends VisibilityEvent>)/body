{
  if (!domainEventHelper.hasEventBusService()) {
    return null;
  }
  final ObjectAdapter[] argumentAdapters=argumentAdaptersFrom(ic);
  final ActionDomainEvent<?> event=domainEventHelper.postEventForAction(AbstractDomainEvent.Phase.HIDE,eventType(),null,getIdentified(),ic.getTarget(),argumentAdapters,null,null);
  if (event != null && event.isHidden()) {
    return "Hidden by subscriber";
  }
  return null;
}
