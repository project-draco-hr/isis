{
  if (!domainEventHelper.hasEventBusService()) {
    return null;
  }
  currentInteraction.set(null);
  final ObjectAdapter[] argumentAdapters=argumentAdaptersFrom(ic);
  final ActionDomainEvent<?> event=domainEventHelper.postEventForAction(eventType(),null,null,AbstractDomainEvent.Phase.HIDE,getIdentified(),ic.getTarget(),argumentAdapters,null);
  if (event != null && event.isHidden()) {
    return "Hidden by subscriber";
  }
  return null;
}
