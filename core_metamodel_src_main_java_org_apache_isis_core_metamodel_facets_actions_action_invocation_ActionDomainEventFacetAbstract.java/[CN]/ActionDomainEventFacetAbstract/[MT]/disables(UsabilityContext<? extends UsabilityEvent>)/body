{
  if (!domainEventHelper.hasEventBusService()) {
    return null;
  }
  currentInteraction.set(null);
  final ActionDomainEvent<?> event=domainEventHelper.postEventForAction(eventType(),null,null,AbstractDomainEvent.Phase.DISABLE,getIdentified(),ic.getTarget(),null);
  if (event != null && event.isDisabled()) {
    return event.getDisabledReason();
  }
  return null;
}
