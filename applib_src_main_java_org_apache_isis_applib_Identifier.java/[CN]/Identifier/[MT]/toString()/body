{
  if (asString == null) {
    final StringBuilder buf=new StringBuilder();
    buf.append(className);
    buf.append('#');
    buf.append(memberName);
    buf.append('(');
    for (int i=0; i < parameterNames.length; i++) {
      if (i > 0) {
        buf.append(", ");
      }
      buf.append(parameterNames[i]);
    }
    buf.append(')');
    asString=buf.toString();
  }
  return asString;
}
