{
  if (a == null && b == null) {
    return true;
  }
 else   if (a == null && b != null) {
    return false;
  }
 else   if (a != null && b == null) {
    return false;
  }
 else   if (a != null && b != null) {
    if (a.length != b.length) {
      return false;
    }
    for (int i=0; i < b.length; i++) {
      if (!a[i].equals(b[i])) {
        return false;
      }
    }
  }
  return true;
}
