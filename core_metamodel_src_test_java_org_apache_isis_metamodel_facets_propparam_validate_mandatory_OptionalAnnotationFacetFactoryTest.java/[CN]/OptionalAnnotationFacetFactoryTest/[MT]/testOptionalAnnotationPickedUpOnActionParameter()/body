{
@edu.umd.cs.findbugs.annotations.SuppressWarnings("UMAC_UNCALLABLE_METHOD_OF_ANONYMOUS_CLASS") class Customer {
    @SuppressWarnings("unused") public void someAction(    @Optional final String foo){
    }
  }
  final Method method=findMethod(Customer.class,"someAction",new Class[]{String.class});
  facetFactory.processParams(method,0,facetHolder);
  final Facet facet=facetHolder.getFacet(MandatoryFacet.class);
  assertNotNull(facet);
  assertTrue(facet instanceof OptionalFacet);
}
