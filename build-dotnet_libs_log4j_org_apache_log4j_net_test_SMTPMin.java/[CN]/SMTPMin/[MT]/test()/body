{
  int i=0;
  cat.debug("Message " + i++);
  cat.debug("Message " + i++,new Exception("Just testing."));
  cat.info("Message " + i++);
  cat.warn("Message " + i++);
  cat.error("Message " + i++);
  cat.log(Priority.FATAL,"Message " + i++);
  Category.shutdown();
  Thread.currentThread().getThreadGroup().list();
}
