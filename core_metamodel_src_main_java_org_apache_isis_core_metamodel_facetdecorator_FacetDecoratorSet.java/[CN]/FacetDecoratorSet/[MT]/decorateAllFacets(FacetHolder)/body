{
  if (isEmpty()) {
    return;
  }
  final Set<Class<? extends Facet>> facetTypes=facetDecoratorByFacetType.keySet();
  for (  final Class<? extends Facet> facetType : facetTypes) {
    final Facet facet=holder.getFacet(facetType);
    if (facet != null) {
      decorateFacet(facet,holder);
    }
  }
}
