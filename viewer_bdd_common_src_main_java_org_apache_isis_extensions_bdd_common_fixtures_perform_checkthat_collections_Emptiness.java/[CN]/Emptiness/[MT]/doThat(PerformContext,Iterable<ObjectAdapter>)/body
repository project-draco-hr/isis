{
  boolean empty=true;
  for (  @SuppressWarnings("unused") final ObjectAdapter eachObject : collection) {
    empty=false;
  }
  if (!assertion.isSatisfiedBy(empty)) {
    CellBinding thatItBinding=performContext.getPeer().getThatItBinding();
    throw StoryBoundValueException.current(thatItBinding,assertion.getErrorMsgIfNotSatisfied());
  }
}
