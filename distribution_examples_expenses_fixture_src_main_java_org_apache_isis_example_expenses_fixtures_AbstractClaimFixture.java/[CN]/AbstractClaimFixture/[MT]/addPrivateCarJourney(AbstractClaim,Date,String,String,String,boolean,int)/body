{
  final PrivateCarJourney item=(PrivateCarJourney)createExpenseItem(claim,ExpenseTypeFixture.PRIVATE_CAR,dateIncurred,description,0.0);
  modifyStandardJourneyFields(item,origin,destination,returnJourney);
  item.modifyTotalMiles(totalMiles);
  item.modifyMileageRate(0.40);
  persist(item);
  return item;
}
