{
  if (LOG.isDebugEnabled()) {
    StringBuffer str=new StringBuffer();
    str.append(message);
    str.append(" ");
    str.append(identifier);
    str.append(" on ");
    str.append(target);
    for (int i=0; i < parameters.length; i++) {
      if (i > 0) {
        str.append(',');
      }
      str.append(parameters[i]);
    }
    return str.toString();
  }
 else {
    return "";
  }
}
