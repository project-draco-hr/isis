{
  if (isPersistent(object) && fullProxy) {
    String[] parameterTypes=pararmeterTypes();
    Data[] parameterObjectData=parameterValues(parameters);
    return connection.getActionHint(NakedObjects.getCurrentSession(),getType().getName(),getName(),parameterTypes,object.getOid(),object.getSpecification().getFullName(),parameterObjectData);
  }
 else {
    return super.getHint(identifier,object,parameters);
  }
}
