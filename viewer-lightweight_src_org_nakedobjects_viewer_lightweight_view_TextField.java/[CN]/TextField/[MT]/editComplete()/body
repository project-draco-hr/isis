{
  if (canChangeValue()) {
    String entry=content.getText();
    if (!entry.equals(value.title().toString())) {
      LOG.debug("Field edited: new data \'" + entry + "\' replacing \'"+ value.title()+ "\'");
      set(entry.toString());
      isValid=true;
      redraw();
      getParent().invalidateLayout();
      isSaved=true;
      return;
    }
  }
}
