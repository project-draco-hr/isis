{
  super.assertFieldContains(fieldName,expectedValue);
  if (getField(fieldName) instanceof TestValue) {
    doc("<p>Note that the field <em>" + fieldName + "</em> in the "+ doc.objectString(getForObject())+ " is now set to '"+ getField(fieldName).getForObject().title()+ "'.");
  }
 else {
    doc("<p>Note that object in the field <em>" + fieldName + "</em> of "+ doc.objectString(getForObject())+ " now has a title of '"+ getField(fieldName).getForObject().title()+ "'.");
  }
}
