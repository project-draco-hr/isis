{
  super.assertFieldDoesNotContain(fieldName,testValue);
  if (getField(fieldName) instanceof TestValue) {
    doc("<p>Note that the field <em>" + fieldName + "</em> in the "+ doc.objectString(getForObject())+ " does not contain a value of '"+ testValue+ "'.");
  }
 else {
    doc("<p>Note that object in the field <em>" + fieldName + "</em> of "+ doc.objectString(getForObject())+ " does not contains an object titled of '"+ getField(fieldName).getForObject().titleString()+ "'.");
  }
}
