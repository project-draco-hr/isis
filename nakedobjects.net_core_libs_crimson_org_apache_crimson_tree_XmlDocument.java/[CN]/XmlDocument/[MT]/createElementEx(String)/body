{
  ElementNode retval;
  if (!XmlNames.isName(tagName)) {
    throw new DomEx(DomEx.INVALID_CHARACTER_ERR);
  }
  if (factory != null) {
    retval=(ElementNode)factory.createElementEx(tagName);
    retval.setTag(tagName);
  }
 else {
    retval=new ElementNode(tagName);
  }
  retval.setOwnerDocument(this);
  return retval;
}
