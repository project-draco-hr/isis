{
  if (ObjectList.class.isAssignableFrom(cls)) {
    return new InstanceCollectionSpecification(this,getRuntimeContext().getAuthenticationSessionProvider(),getRuntimeContext().getServicesProvider(),getRuntimeContext().getObjectInstantiator());
  }
  return new JavaSpecification(cls,this,getRuntimeContext().getAuthenticationSessionProvider(),getRuntimeContext().getSpecificationLookup(),getRuntimeContext().getServicesProvider(),getRuntimeContext().getAdapterMap(),getRuntimeContext().getObjectInstantiator(),getRuntimeContext().getDependencyInjector(),getRuntimeContext().getQuerySubmitter());
}
