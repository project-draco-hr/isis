{
  if (!getName().isEmpty()) {
    return getName().title();
  }
  if (!getStreetAddress().isEmpty()) {
    return getStreetAddress().title();
  }
  if (!getCity().isEmpty()) {
    return getCity().title();
  }
  if (!getCountry().isEmpty()) {
    return getCountry().title();
  }
  return new Title("New location");
}
