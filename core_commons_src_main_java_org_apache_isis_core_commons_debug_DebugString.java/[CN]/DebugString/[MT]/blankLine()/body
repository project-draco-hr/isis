{
  final int length=string.length();
  if (length == 0) {
    return;
  }
  final boolean hasLineEnding=string.charAt(length - 1) == '\n';
  if (!hasLineEnding) {
    string.append('\n');
    string.append('\n');
    newLine=true;
  }
 else {
    final boolean hasDoubleLineEnding=length >= 2 && string.charAt(length - 2) != '\n';
    if (hasDoubleLineEnding) {
      string.append('\n');
      newLine=true;
    }
  }
}
