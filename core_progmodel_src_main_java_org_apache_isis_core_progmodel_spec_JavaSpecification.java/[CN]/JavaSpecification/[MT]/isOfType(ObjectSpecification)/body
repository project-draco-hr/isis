{
  if (specification == this) {
    return true;
  }
 else {
    if (interfaces != null) {
      for (int i=0, len=interfaces.size(); i < len; i++) {
        if (interfaces.get(i).isOfType(specification)) {
          return true;
        }
      }
    }
    if (superClassSpecification != null) {
      return superClassSpecification.isOfType(specification);
    }
  }
  return false;
}
