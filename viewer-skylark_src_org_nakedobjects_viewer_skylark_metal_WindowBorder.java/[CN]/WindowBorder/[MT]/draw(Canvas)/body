{
  super.draw(canvas);
  if (isTransient(this)) {
    int height=top - 2;
    int x=100;
    Image icon=ImageFactory.getInstance().createIcon("transient",height,null);
    if (icon == null) {
      canvas.drawText("*",x,getBaseline(),Style.BLACK,Style.NORMAL);
    }
 else {
      canvas.drawIcon(icon,x,1,height,height);
    }
  }
}
