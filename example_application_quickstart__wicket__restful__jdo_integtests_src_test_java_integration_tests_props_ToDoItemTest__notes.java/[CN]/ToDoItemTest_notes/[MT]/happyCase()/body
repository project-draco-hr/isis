{
  final String newNotes="Lorem ipsum yada yada";
  toDoItem.setNotes(newNotes);
  assertThat(toDoItem.getNotes(),is(newNotes));
  @SuppressWarnings("unchecked") final PropertyChangedEvent<ToDoItem,String> ev=toDoItemSubscriptions.mostRecentlyReceivedEvent(PropertyChangedEvent.class);
  assertThat(ev,is(nullValue()));
}
