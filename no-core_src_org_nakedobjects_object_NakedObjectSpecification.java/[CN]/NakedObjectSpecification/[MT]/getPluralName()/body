{
  String pluralName=reflector.pluralName();
  if (pluralName != null) {
    return pluralName;
  }
 else {
    String name=getSingularName();
    if (name.endsWith("y")) {
      name=name.substring(0,name.length() - 1) + "ies";
    }
 else     if (name.endsWith("s")) {
      name+="es";
    }
 else {
      name+='s';
    }
    return name;
  }
}
