{
  final List<String> parts=Lists.newArrayList();
  parts.add(COMMON_TNS);
  parts.add(COMMON_SCHEMA_LOCATION);
  FluentIterable.from(gridNormalizerServices).transform(new Function<GridNormalizerService,Void>(){
    @Nullable @Override public Void apply(    final GridNormalizerService gridNormalizerService){
      parts.add(gridNormalizerService.tns());
      parts.add(gridNormalizerService.schemaLocation());
      return null;
    }
  }
);
  return Joiner.on(" ").join(parts);
}
