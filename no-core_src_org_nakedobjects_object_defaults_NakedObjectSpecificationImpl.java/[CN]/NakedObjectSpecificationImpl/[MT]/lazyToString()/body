{
  StringBuffer s=new StringBuffer();
  s.append("NakedObjectSpecification");
  if (reflector != null) {
    s.append(" [name=");
    s.append(getFullName());
    s.append(",reflector=");
    s.append(reflector);
    s.append("]");
  }
 else {
    s.append("[no relector set up]");
  }
  s.append("  " + Long.toHexString(super.hashCode()).toUpperCase());
  return s.toString();
}
