{
  Image image;
  image=loadAsResource(name);
  if (image == null && alsoLoadAsFiles) {
    image=loadAsFile(name);
  }
  if (image == null) {
    if (name.indexOf("" + SMALL) > 0) {
      image=smallUnknown;
    }
 else     if (name.indexOf("" + NORMAL) > 0) {
      image=largeUnknown;
    }
  }
  if (image == null) {
    LOG.error("No image loaded for: " + name);
  }
  return image;
}
