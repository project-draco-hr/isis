{
  NakedObject loadedObject=objectLoader.getAdapterFor(oid);
  if (loadedObject != null) {
    return loadedObject;
  }
 else {
    Object pojo=objectInstances.get(oid);
    if (pojo == null) {
      return null;
    }
    NakedObject adapter;
    adapter=objectLoader.recreateAdapterForPersistent(oid,pojo);
    return adapter;
  }
}
