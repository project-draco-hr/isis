{
  if (this.msg == null) {
    errorHandler.error("Message object not configured.");
    return false;
  }
  if (this.evaluator == null) {
    errorHandler.error("No TriggeringEventEvaluator is set for appender [" + name + "].");
    return false;
  }
  if (this.layout == null) {
    errorHandler.error("No layout set for appender named [" + name + "].");
    return false;
  }
  return true;
}
