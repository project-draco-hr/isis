{
  if (specification == null) {
    return "";
  }
 else {
    String title=specification.getTitle().title(this);
    if (title == null && !isResolved()) {
      title=specification.unresolvedTitle(this);
    }
    if (title == null) {
      title="A " + specification.getSingularName().toLowerCase();
    }
    return title;
  }
}
