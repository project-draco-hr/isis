{
  View subview=getSubviews()[0];
  int top=subview.getPadding().getTop();
  for (  View view : subview.getSubviews()) {
    String fieldName=((FieldContent)view.getContent()).getFieldName();
    canvas.drawText(fieldName + ":",0,view.getLocation().getY() + top + view.getBaseline(),Toolkit.getColor(ColorsAndFonts.COLOR_PRIMARY1),Toolkit.getText(ColorsAndFonts.TEXT_LABEL));
  }
  super.draw(canvas);
}
