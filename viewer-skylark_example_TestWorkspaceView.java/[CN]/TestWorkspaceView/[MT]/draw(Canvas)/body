{
  canvas.drawRectangle(10,10,100,300,new Color(0x0044ff));
  Bounds bounds=getBounds();
  canvas.drawRectangle(0,0,bounds.getWidth(),bounds.getHeight(),new Color(0x0044ff));
  canvas.drawText("Test Workspace",20,40,new Color(0x0044ff),Style.TITLE);
  Canvas subcanvas=canvas.createSubcanvas(view.getBounds());
  view.draw(subcanvas);
}
