{
  final Method method=findMethod(cls,OBJECT,CHOICES_PREFIX + capitalizedName,null,NO_PARAMETERS_TYPES);
  if (method == null) {
    return;
  }
  methodRemover.removeMethod(method);
  propertyFacets.add(new PropertyChoicesFacetViaMethod(method,returnType,property,getSpecificationLookup(),getAdapterMap()));
}
