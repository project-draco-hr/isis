{
  Task task=null;
  for (int i=crumbs.size() - 1; i >= 0; i--) {
    final Object crumb=crumbs.get(i);
    if (crumb instanceof TaskCrumb) {
      final TaskCrumb taskCrumb=(TaskCrumb)crumb;
      final String id=taskCrumb.getTask().getId();
      if (taskId.equals(id)) {
        task=taskCrumb.getTask();
        break;
      }
    }
  }
  return task;
}
