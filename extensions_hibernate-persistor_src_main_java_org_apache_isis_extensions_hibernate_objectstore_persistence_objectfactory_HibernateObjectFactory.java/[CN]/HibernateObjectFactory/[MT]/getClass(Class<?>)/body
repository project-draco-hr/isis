{
  if (PersistentCollection.class.isAssignableFrom(cls)) {
    if (List.class.isAssignableFrom(cls)) {
      return List.class;
    }
    if (Set.class.isAssignableFrom(cls)) {
      return Set.class;
    }
    if (Map.class.isAssignableFrom(cls)) {
      return Map.class;
    }
  }
  return cls;
}
