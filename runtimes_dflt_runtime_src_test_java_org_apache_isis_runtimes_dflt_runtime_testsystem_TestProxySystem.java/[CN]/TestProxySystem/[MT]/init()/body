{
  reflector.setRuntimeContext(new RuntimeContextFromSession());
  IsisSessionFactory sessionFactory=new IsisSessionFactoryDefault(DeploymentType.EXPLORATION,configuration,noopTemplateImageLoader,reflector,authenticationManager,authorizationManager,userProfileLoader,persistenceSessionFactory,servicesList);
  persistenceSession.setSpecificationLoader(reflector);
  sessionFactory.init();
  context=IsisContextStatic.createRelaxedInstance(sessionFactory);
  IsisContext.openSession(new TestProxySession());
}
