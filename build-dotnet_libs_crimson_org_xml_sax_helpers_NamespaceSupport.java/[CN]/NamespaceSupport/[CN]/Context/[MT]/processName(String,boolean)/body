{
  String name[];
  Hashtable table;
  if (isAttribute) {
    table=elementNameTable;
  }
 else {
    table=attributeNameTable;
  }
  name=(String[])table.get(qName);
  if (name != null) {
    return name;
  }
  name=new String[3];
  int index=qName.indexOf(':');
  if (index == -1) {
    if (isAttribute || defaultNS == null) {
      name[0]="";
    }
 else {
      name[0]=defaultNS;
    }
    name[1]=qName.intern();
    name[2]=name[1];
  }
 else {
    String prefix=qName.substring(0,index);
    String local=qName.substring(index + 1);
    String uri;
    if ("".equals(prefix)) {
      uri=defaultNS;
    }
 else {
      uri=(String)prefixTable.get(prefix);
    }
    if (uri == null) {
      return null;
    }
    name[0]=uri;
    name[1]=local.intern();
    name[2]=qName.intern();
  }
  table.put(name[2],name);
  tablesDirty=true;
  return name;
}
