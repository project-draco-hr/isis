{
  super.drop(drag);
  NakedObject source=((ObjectContent)drag.getSourceContent()).getObject();
  if (source.getObject() instanceof NakedClass) {
    if (drag.isShift()) {
      getContext().getClasses().addElement(source);
    }
  }
 else {
    if (!drag.isShift()) {
      getContext().getObjects().addElement(source);
    }
  }
}
