{
  writer.print("<div");
  if (className != null) {
    writer.print(" class=\"");
    writer.print(className);
    writer.print("\"");
  }
  if (id != null) {
    writer.print(" id=\"");
    writer.print(id);
    writer.print("\"");
  }
  if (description != null) {
    writer.print(" title=\"");
    writer.print(description);
    writer.print("\"");
  }
  writer.print(">");
}
