{
  if (cls == null) {
    return;
  }
  final Method[] methods=cls.getMethods();
  for (int j=0; j < methods.length; j++) {
    final Method method=methods[j];
    final Ignore annotation=getAnnotation(method,Ignore.class);
    if (annotation != null) {
      methodRemover.removeMethod(method);
    }
  }
}
