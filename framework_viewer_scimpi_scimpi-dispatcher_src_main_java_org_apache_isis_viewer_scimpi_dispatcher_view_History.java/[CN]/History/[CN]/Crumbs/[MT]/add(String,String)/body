{
  for (  final Crumb crumb : crumbs) {
    if (crumb.link.equals(link)) {
      crumbs.remove(crumb);
      crumbs.add(crumb);
      return;
    }
  }
  final Crumb crumb=new Crumb();
  crumb.name=name;
  crumb.link=link;
  crumbs.add(crumb);
  if (crumbs.size() > MAXIMUM_SIZE) {
    crumbs.remove(0);
  }
}
