{
  final PropertyMemento propertyMemento=scalarModel.getPropertyMemento();
  final OneToOneAssociation property=propertyMemento.getProperty();
  final ObjectAdapter parentAdapter=scalarModel.parentObjectAdapterMemento.getObjectAdapter(ConcurrencyChecking.NO_CHECK);
  final ObjectAdapter[] choices=InteractionUtils.withFiltering(new Callable<ObjectAdapter[]>(){
    @Override public ObjectAdapter[] call() throws Exception {
      return property.getAutoComplete(parentAdapter,searchArg,authenticationSession,deploymentCategory,InteractionInitiatedBy.USER);
    }
  }
);
  return choicesAsList(choices);
}
