{
  final PropertyMemento propertyMemento=scalarModel.getPropertyMemento();
  final OneToOneAssociation property=propertyMemento.getProperty();
  ObjectAdapter parentAdapter=scalarModel.parentObjectAdapterMemento.getObjectAdapter(ConcurrencyChecking.NO_CHECK);
  final ObjectAdapter[] choices=property.getChoices(parentAdapter,InteractionInitiatedBy.USER);
  return choicesAsList(choices);
}
