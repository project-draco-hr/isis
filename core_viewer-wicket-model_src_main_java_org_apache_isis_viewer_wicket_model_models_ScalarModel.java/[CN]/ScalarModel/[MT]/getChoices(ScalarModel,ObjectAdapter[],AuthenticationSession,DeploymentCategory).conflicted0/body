{
  final PropertyMemento propertyMemento=scalarModel.getPropertyMemento();
  final OneToOneAssociation property=propertyMemento.getProperty();
  final ObjectAdapter[] choices=property.getChoices(scalarModel.parentObjectAdapterMemento.getObjectAdapter(ConcurrencyChecking.NO_CHECK),authenticationSession,deploymentCategory);
  return choicesAsList(choices);
}
