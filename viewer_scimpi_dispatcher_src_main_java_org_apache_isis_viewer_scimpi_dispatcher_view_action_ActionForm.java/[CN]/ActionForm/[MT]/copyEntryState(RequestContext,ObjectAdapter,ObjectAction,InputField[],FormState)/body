{
  for (  final InputField field : fields) {
    final FieldEditState fieldState=entryState.getField(field.getName());
    if (fieldState != null) {
      if (field.isEditable()) {
        String entry;
        entry=fieldState.getEntry();
        field.setValue(entry);
      }
      field.setErrorText(fieldState.getError());
    }
  }
}
