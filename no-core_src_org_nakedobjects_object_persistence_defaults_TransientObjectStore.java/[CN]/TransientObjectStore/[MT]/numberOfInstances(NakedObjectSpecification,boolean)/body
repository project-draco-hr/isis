{
  int noInstances=0;
  Enumeration e=elements(spec);
  while (e.hasMoreElements()) {
    NakedObject data=(NakedObject)e.nextElement();
    if (spec.equals(data.getSpecification())) {
      noInstances++;
    }
  }
  LOG.debug("numberOfInstances of " + spec + " = "+ noInstances);
  return noInstances;
}
