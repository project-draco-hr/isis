{
  super(holder);
  this.eventType=eventType;
  this.method=method;
  this.onType=onType;
  this.returnType=returnType;
  this.actionInteractionFacet=actionInteractionFacet;
  this.runtimeContext=runtimeContext;
  this.adapterManager=adapterManager;
