{
  row=cell=0;
  final View[] rows=table.getSubviews();
  if (rows.length > 0) {
    row=0;
    final View[] cells=rows[0].getSubviews();
    for (int j=0; j < cells.length; j++) {
      if (cells[j].canFocus()) {
        cells[cell].markDamaged();
        cell=j;
        cells[j].markDamaged();
        return;
      }
    }
  }
}
