{
  if (element.validator != null)   return element.validator;
  if (element.model != null)   return new ChildrenValidator(element);
  if (element.contentType == null || strANY == element.contentType)   element.validator=ElementValidator.ANY;
 else   if (strEMPTY == element.contentType)   element.validator=EMPTY;
 else   element.validator=new MixedValidator(element);
  return element.validator;
}
