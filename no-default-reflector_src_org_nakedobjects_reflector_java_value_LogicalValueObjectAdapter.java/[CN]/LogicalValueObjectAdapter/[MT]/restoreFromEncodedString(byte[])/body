{
  if (data.length != 1) {
    throw new NakedObjectRuntimeException("Invalid data for logical, expected one byte, got " + data.length);
  }
  if (data[0] == 'T') {
    set();
  }
 else   if (data[0] == 'F') {
    reset();
  }
 else {
    throw new NakedObjectRuntimeException("Invalid data for logical, expected 'T' or 'F', but  got " + data[0]);
  }
}
