{
  objectStore=new InMemoryObjectStore(){
    @Override protected InMemoryPersistenceSessionFactory getInMemoryPersistenceSessionFactory(){
      return mockInMemoryPersistenceSessionFactory;
    }
    @Override protected PersistenceSession getPersistenceSession(){
      return mockPersistenceSession;
    }
    @Override protected void recreateAdapters(){
      recreatedAdapters=true;
    }
  }
;
}
