{
  final StringBuffer buff=new StringBuffer();
  buff.append(entry.getKey()).append(":");
  final List<String> roles=entry.getValue();
  for (int j=0; j < roles.size(); j++) {
    buff.append(roles.get(j));
    if (j < roles.size() - 1) {
      buff.append("|");
    }
  }
  buffWriter.write(buff.toString());
  buffWriter.newLine();
}
