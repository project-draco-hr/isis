{
  text.append("    " + member.toString() + "\n");
  String desc=about.getDescription();
  if (desc != null && !desc.equals("")) {
    text.append("       desc:  " + desc + "\n");
  }
  String aboutDesc=about.debug();
  if (aboutDesc != null && !aboutDesc.equals("")) {
    text.append("       about: " + aboutDesc + "\n");
  }
}
