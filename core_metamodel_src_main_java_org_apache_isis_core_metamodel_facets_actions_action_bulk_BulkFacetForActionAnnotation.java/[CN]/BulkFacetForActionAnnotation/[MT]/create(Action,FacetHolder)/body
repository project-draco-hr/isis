{
  if (action == null) {
    return null;
  }
  final InvokeOn invokeOn=action.invokeOn();
  if (invokeOn == null) {
    return null;
  }
switch (invokeOn) {
case OBJECT_ONLY:
    return null;
case OBJECT_AND_COLLECTION:
  return new BulkFacetForActionAnnotation(InvokeOn.from(invokeOn),holder);
case COLLECTION_ONLY:
return new BulkFacetForActionAnnotation(InvokeOn.from(invokeOn),holder);
}
return null;
}
