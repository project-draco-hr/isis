{
  if (buildInvalid) {
    getFeedbackManager().setBusy(this,null);
    buildInvalid=false;
    if (subviews().length == 0) {
      buildNewView();
    }
 else {
      buildModifiedView();
    }
    for (    final View view : subviews()) {
      view.getSubviews();
    }
    getFeedbackManager().clearBusy(this);
    buildCount++;
  }
}
