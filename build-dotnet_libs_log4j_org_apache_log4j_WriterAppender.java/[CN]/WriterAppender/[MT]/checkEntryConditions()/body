{
  if (this.closed) {
    LogLog.warn("Not allowed to write to a closed appender.");
    return false;
  }
  if (this.qw == null) {
    errorHandler.error("No output stream or file set for the appender named [" + name + "].");
    return false;
  }
  if (this.layout == null) {
    errorHandler.error("No layout set for the appender named [" + name + "].");
    return false;
  }
  return true;
}
