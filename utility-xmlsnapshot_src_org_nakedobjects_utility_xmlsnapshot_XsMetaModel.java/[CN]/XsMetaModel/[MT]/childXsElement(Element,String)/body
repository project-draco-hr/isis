{
  NodeList nodeList=parentXsElement.getElementsByTagNameNS(XsMetaModel.W3_ORG_XS_URI,localName);
  if (nodeList.getLength() > 0) {
    return (Element)nodeList.item(0);
  }
  Element childXsElement=createXsElement(helper.docFor(parentXsElement),localName);
  parentXsElement.appendChild(childXsElement);
  return childXsElement;
}
