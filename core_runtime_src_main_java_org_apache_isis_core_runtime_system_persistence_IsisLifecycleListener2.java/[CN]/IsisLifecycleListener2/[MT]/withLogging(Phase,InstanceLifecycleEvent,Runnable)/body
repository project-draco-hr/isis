{
  if (IsisLifecycleListener.LOG.isDebugEnabled()) {
    IsisLifecycleListener.LOG.debug(logString(phase,LoggingLocation.ENTRY,event));
  }
  try {
    runnable.run();
  }
  finally {
    if (IsisLifecycleListener.LOG.isDebugEnabled()) {
      IsisLifecycleListener.LOG.debug(logString(phase,LoggingLocation.EXIT,event));
    }
  }
}
