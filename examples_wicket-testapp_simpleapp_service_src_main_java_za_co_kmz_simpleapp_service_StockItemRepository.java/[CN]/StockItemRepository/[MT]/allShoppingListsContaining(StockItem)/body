{
  List<ShoppingList> lists=allShoppingLists();
  if (lists.size() == 0) {
    return null;
  }
  List<ShoppingList> retList=new ArrayList<ShoppingList>();
  for (  ShoppingList list : lists) {
    if (list.getItems().contains(item)) {
      retList.add(list);
    }
  }
  if (retList.size() > 0) {
    return retList;
  }
  return null;
}
