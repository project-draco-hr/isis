{
  if (order == null) {
    return;
  }
  Vector sorted=new Vector(elements.length);
  outer:   for (int j=0; j < elements.length; j++) {
    NakedObject element=elements[j];
    order.init(element);
    int i=0;
    for (Enumeration f=sorted.elements(); f.hasMoreElements(); ) {
      NakedObject sortedElement=(NakedObject)f.nextElement();
      if (sortedElement != null && (order.compare(sortedElement) > 0 ^ reverse)) {
        sorted.insertElementAt(element,i);
        continue outer;
      }
      i++;
    }
    sorted.addElement(element);
  }
  sorted.copyInto(elements);
}
