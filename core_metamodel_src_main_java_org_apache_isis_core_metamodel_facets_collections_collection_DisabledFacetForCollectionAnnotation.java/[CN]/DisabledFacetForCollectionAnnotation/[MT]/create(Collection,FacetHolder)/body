{
  final EditPolicy editPolicy=collection.editing();
  final String disabledReason=collection.editingDisabledReason();
switch (editPolicy) {
case AS_CONFIGURED:
    return null;
case DISABLED:
  return new DisabledFacetForCollectionAnnotation(disabledReason,holder);
case ENABLED:
return null;
}
return null;
}
