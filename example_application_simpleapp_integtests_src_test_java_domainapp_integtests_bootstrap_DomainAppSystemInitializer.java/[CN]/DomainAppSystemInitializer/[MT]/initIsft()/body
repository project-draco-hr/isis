{
  IsisSystemForTest isft=IsisSystemForTest.getElseNull();
  if (isft == null) {
    isft=new IsisSystemForTest.Builder().withLoggingAt(org.apache.log4j.Level.INFO).with(new DomainAppAppManifest()).with(new IsisConfigurationForJdoIntegTests()).build();
    isft.setUpSystem();
    IsisSystemForTest.set(isft);
  }
}
