{
  if (name == null) {
    return findImage(DEFAULT_IMAGE,EXTENSIONS);
  }
  String imageName=(String)images.get(name);
  if (imageName != null) {
    return imageName;
  }
  imageName=findImage(name,EXTENSIONS);
  if (imageName != null) {
    images.put(name,imageName);
    return imageName;
  }
  return image((String)null);
}
