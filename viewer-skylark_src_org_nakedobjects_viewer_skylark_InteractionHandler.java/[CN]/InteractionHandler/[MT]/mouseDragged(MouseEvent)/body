{
  if (isBusy(null)) {
    return;
  }
  try {
    if (canDrag) {
      spy.reset();
      viewer.translate(me);
      if (drag == null) {
        dragStart(me);
        redraw();
      }
 else {
        drag(me);
        redraw();
      }
    }
  }
 catch (  Exception e) {
    log("mouseDragged",e);
  }
}
