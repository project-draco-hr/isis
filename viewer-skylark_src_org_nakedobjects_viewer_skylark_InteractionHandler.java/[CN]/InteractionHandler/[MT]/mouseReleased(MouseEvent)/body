{
  if (isBusy(null)) {
    return;
  }
  try {
    if (drag != null) {
      mouseDragged(me);
      Location location=new Location(me.getPoint());
      drag.drag(viewer,location,me.getModifiers());
      viewer.clearStatus();
      drag.end(viewer);
      redraw();
      drag=null;
    }
  }
 catch (  Exception e) {
    log("mouseReleased",e);
  }
}
