{
  Action action=null;
  try {
    action=getAction(name,new NakedObjectSpecification[0]);
  }
 catch (  NakedObjectSpecificationException e) {
    throw new NakedAssertionFailedError(e.getMessage());
  }
  if (action == null) {
    throw new NakedAssertionFailedError("Method not found: " + name);
  }
  return action;
}
