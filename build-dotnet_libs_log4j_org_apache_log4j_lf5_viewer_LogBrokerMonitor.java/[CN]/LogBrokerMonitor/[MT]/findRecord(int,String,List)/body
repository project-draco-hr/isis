{
  if (startRow < 0) {
    startRow=0;
  }
 else {
    startRow++;
  }
  int len=records.size();
  for (int i=startRow; i < len; i++) {
    if (matches((LogRecord)records.get(i),searchText)) {
      return i;
    }
  }
  len=startRow;
  for (int i=0; i < len; i++) {
    if (matches((LogRecord)records.get(i),searchText)) {
      return i;
    }
  }
  return -1;
}
