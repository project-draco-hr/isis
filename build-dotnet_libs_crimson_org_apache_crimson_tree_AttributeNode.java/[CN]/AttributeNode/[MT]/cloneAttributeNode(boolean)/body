{
  try {
    AttributeNode attr=makeClone();
    if (deep) {
      Node node;
      for (int i=0; (node=item(i)) != null; i++) {
        node=node.cloneNode(true);
        attr.appendChild(node);
      }
    }
    return attr;
  }
 catch (  DOMException e) {
    throw new RuntimeException(getMessage("A-002"));
  }
}
