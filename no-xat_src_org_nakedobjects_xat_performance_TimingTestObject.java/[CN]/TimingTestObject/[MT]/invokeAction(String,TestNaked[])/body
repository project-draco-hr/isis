{
  String parameterList="";
  for (int i=0, l=parameters.length; i < l; i++) {
    parameterList+=(i > 0 ? "," : "") + parameters[i].getTitle();
  }
  Timer timer=new Timer("action '" + name + "' with "+ parameters);
  timer.userDelay(2,4);
  timer.start();
  TestNaked result=super.invokeAction(name,parameters);
  timer.stop();
  doc.record(timer);
  return result;
}
