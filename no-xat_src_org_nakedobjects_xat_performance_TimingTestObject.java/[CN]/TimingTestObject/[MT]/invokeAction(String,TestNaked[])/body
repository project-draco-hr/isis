{
  String parameterList="";
  for (int i=0, l=parameters.length; i < l; i++) {
    parameterList+=(i > 0 ? "," : "") + parameters[i].getTitle();
  }
  Profiler timer=start("action '" + name + "' with "+ parameters);
  TestObject result=super.invokeAction(name,parameters);
  stop(timer);
  return result;
}
