{
  String error="";
  if (missingFields.length() > 0) {
    if (error.length() > 0) {
      error+="; ";
    }
    error+="Fields needed: " + missingFields;
  }
  if (invalidFields.length() > 0) {
    if (error.length() > 0) {
      error+="; ";
    }
    error+="Invalid fields: " + invalidFields;
  }
  return error;
}
