{
  if (name.equals(NAMESPACES)) {
    checkNotParsing("feature",name);
    namespaces=state;
    if (!namespaces && !prefixes) {
      prefixes=true;
    }
  }
 else   if (name.equals(NAMESPACE_PREFIXES)) {
    checkNotParsing("feature",name);
    prefixes=state;
    if (!prefixes && !namespaces) {
      namespaces=true;
    }
  }
 else   if (name.equals(VALIDATION)) {
    checkNotParsing("feature",name);
    if (validation != state) {
      parser=null;
    }
    validation=state;
  }
 else   if (name.equals(STRING_INTERNING)) {
    if (state == false) {
      throw new SAXNotSupportedException("Feature: " + name + " State: false");
    }
  }
 else   if (name.equals(EXTERNAL_GENERAL) || name.equals(EXTERNAL_PARAMETER) || name.equals(LEXICAL_PARAMETER_ENTITIES)) {
    throw new SAXNotSupportedException("Feature: " + name);
  }
 else {
    throw new SAXNotRecognizedException("Feature: " + name);
  }
}
