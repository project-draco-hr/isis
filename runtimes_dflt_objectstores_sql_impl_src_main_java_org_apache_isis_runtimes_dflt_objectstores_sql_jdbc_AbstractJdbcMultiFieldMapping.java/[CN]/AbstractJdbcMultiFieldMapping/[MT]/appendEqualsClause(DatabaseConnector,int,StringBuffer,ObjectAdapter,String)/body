{
  final ObjectAdapter fieldValue=field.get(object);
  final Object o=(fieldValue == null) ? null : fieldValue.getObject();
  for (int i=0; i < columnCount; i++) {
    sql.append(columnName(i) + condition + "?");
    if (i < columnCount - 1) {
      sql.append(", ");
    }
    connector.addToQueryValues(o);
  }
}
