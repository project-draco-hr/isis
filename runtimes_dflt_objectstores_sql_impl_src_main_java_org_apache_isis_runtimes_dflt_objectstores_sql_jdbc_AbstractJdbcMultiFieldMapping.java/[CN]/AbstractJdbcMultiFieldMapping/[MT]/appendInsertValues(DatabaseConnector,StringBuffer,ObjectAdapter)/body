{
  ObjectAdapter fieldValue=field.get(object);
  for (int i=0; i < columnCount; i++) {
    if (fieldValue == null) {
      sql.append("NULL");
    }
 else {
      sql.append("?");
      if (i < columnCount - 1) {
        sql.append(", ");
      }
      connector.addToQueryValues(preparedStatementObject(i,fieldValue));
    }
  }
}
