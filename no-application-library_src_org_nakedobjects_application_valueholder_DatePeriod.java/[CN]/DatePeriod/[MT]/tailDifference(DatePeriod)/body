{
  if (!this.overlaps(arg)) {
    throw new IllegalArgumentException("No overlap");
  }
 else {
    if (this.endsAfter(arg)) {
      getStart().setValue(arg.getEnd());
      getStart().add(0,0,1);
    }
 else {
      getStart().setValue(getEnd());
      getStart().add(0,0,1);
      getEnd().setValue(arg.getEnd());
    }
  }
}
