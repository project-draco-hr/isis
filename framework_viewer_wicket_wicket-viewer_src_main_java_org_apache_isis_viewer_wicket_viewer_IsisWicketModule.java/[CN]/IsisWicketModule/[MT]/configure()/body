{
  bind(ComponentFactoryRegistry.class).to(ComponentFactoryRegistryDefault.class);
  bind(PageClassRegistry.class).to(PageClassRegistryDefault.class);
  bind(PageClassList.class).to(PageClassListDefault.class);
