{
  final ObjectMember objectMember=processMemberContext.getFacetHolder();
  final PropertyOrCollectionAccessorFacet accessorFacet=objectMember.getFacet(PropertyOrCollectionAccessorFacet.class);
  if (accessorFacet != null) {
    final PropertyDomainEventFacet facet=new PropertyDomainEventFacetDefault(PropertyDomainEvent.Default.class,accessorFacet,servicesInjector,getSpecificationLoader(),objectMember);
    FacetUtil.addFacet(facet);
  }
}
