{
  NodeBase current=this;
  Attr value=null;
  do {
    if (current instanceof ElementNode2) {
      ElementNode2 e=(ElementNode2)current;
      if ((value=e.getAttributeNode(name)) != null)       break;
    }
    current=current.getParentImpl();
  }
 while (current != null);
  if (value != null)   return value.getValue();
  return null;
}
