{
  try {
    if (getDatastoreMapping(0).getObject(resultSet,exprIndex[0]) == null) {
      return null;
    }
  }
 catch (  Exception e) {
  }
  String name=getDatastoreMapping(0).getString(resultSet,exprIndex[0]);
