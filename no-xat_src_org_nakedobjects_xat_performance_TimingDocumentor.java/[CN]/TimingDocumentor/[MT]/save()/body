{
  File dir=new File(directory);
  System.out.println(dir.getAbsolutePath());
  if (!dir.exists()) {
    dir.mkdirs();
  }
  PrintWriter writer=null;
  try {
    writer=new PrintWriter(new OutputStreamWriter(new FileOutputStream(new File(directory,"timing" + "-xat.data"))));
    for (int i=0, max=timings.size(); i < max; i++) {
      Timer timer=(Timer)timings.elementAt(i);
      writer.println(timer.logRecord());
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
 finally {
    if (writer != null) {
      writer.close();
    }
  }
}
