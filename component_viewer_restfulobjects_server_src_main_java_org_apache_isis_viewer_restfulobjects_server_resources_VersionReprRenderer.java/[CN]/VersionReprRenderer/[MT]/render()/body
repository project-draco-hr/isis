{
  if (includesSelf) {
    withLink(Rel.SELF,"version");
    withLink(Rel.UP,"");
  }
  representation.mapPut("specVersion",RestfulObjectsApplication.SPEC_VERSION);
  representation.mapPut("implVersion",versionFromManifest());
  putOptionalCapabilities();
  putExtensions();
  return representation;
}
