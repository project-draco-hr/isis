{
  if (contentHandler == null) {
    contentHandler=nullHandler;
  }
  if (errHandler == null) {
    errHandler=nullHandler;
  }
  if (dtdHandler == null) {
    dtdHandler=nullHandler;
  }
  if (lexicalHandler == null) {
    lexicalHandler=nullHandler;
  }
  if (declHandler == null) {
    declHandler=nullHandler;
  }
}
