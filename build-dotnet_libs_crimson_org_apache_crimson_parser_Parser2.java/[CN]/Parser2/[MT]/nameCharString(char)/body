{
  int i=1;
  nameTmp[0]=c;
  for (; ; ) {
    if ((c=in.getNameChar()) == 0)     break;
    if (i >= nameTmp.length) {
      char tmp[]=new char[nameTmp.length + 10];
      System.arraycopy(nameTmp,0,tmp,0,nameTmp.length);
      nameTmp=tmp;
    }
    nameTmp[i++]=c;
  }
  return nameCache.lookupEntry(nameTmp,i);
}
