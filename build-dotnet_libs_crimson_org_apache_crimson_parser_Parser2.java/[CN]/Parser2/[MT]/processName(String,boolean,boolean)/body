{
  String parts[]=nsSupport.processName(qName,namePartsTmp,isAttribute);
  if (parts == null) {
    parts=new String[3];
    parts[0]="";
    String localName=XmlNames.getLocalPart(qName);
    parts[1]=localName != null ? localName.intern() : "";
    parts[2]=qName.intern();
    String messageId="P-084";
    Object[] parameters=new Object[]{qName};
    if (useException) {
      throw new SAXParseException(messages.getMessage(locale,messageId,parameters),locator);
    }
    error(messageId,parameters);
  }
  return parts;
}
