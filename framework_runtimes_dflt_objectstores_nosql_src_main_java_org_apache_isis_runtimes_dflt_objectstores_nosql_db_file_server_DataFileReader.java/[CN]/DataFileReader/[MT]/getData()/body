{
  try {
    final StringBuffer buffer=new StringBuffer();
    String line;
    while ((line=reader.readLine()) != null) {
      buffer.append(line);
      buffer.append("\n");
    }
    return buffer.toString();
  }
 catch (  final IOException e) {
    throw new FileServerException("Failed to read data for " + id,e);
  }
}
