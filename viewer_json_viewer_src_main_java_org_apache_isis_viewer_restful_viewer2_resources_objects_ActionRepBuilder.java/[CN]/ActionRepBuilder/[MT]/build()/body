{
  putSelfIfRequired();
  putTypeRep();
  putMemberTypeRep();
  representation.put("actionType",objectMember.getType());
  representation.put("numParameters",objectMember.getParameterCount());
  putValueIfRequired();
  putDisabledReason();
  putMutatorsIfRequired();
  putDetailsIfRequired();
  return representation;
}
