{
  actions.addElement("saveObject " + object);
  return new SaveObjectCommand(){
    @Override public void execute(    final PersistenceCommandContext context) throws ObjectPersistenceException {
    }
    @Override public String toString(){
      return "DestroyObjectCommand " + object.toString();
    }
    @Override public ObjectAdapter onObject(){
      return object;
    }
  }
;
}
