{
  writer.appendHtml("<tr>");
  writer.appendHtml("<th></th>");
  final String[] columnHeaders=headers;
  for (  final String columnHeader : columnHeaders) {
    if (columnHeader != null) {
      writer.appendHtml("<th>" + columnHeader + "</th>");
    }
  }
  writer.appendHtml("<th>" + "</th>");
  writer.appendHtml("</tr>");
}
