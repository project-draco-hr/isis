{
  IsisSystemForTest isft=IsisSystemForTest.getElseNull();
  if (isft == null) {
    isft=new IsisSystemForTest.Builder().withLoggingAt(org.apache.log4j.Level.INFO).with(new DomainAppGlobSpec()).with(new IsisConfigurationForJdoIntegTests()).build().setUpSystem();
    IsisSystemForTest.set(isft);
  }
}
