{
  if (argv.length == 2)   init(argv[0],argv[1],null,null);
 else   if (argv.length == 4)   init(argv[0],argv[1],argv[2],argv[3]);
 else   Usage("Wrong number of arguments.");
  NDC.push("some context");
  double delta;
  String msg="ABCDEGHIJKLMNOPQRSTUVWXYZabcdeghijklmnopqrstuvwxyz1234567890";
  if (delay <= 0)   delta=NoDelayLoop(cat,msg);
 else   delta=DelayedLoop(cat,msg);
  System.out.print((int)delta);
  Category.shutdown();
}
