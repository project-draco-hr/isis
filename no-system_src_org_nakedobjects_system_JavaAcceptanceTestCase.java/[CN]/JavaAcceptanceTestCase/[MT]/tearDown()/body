{
  LOG.info("test tear down " + getName());
  methodProfiler.reset();
  methodProfiler.start();
  super.tearDown();
  methodProfiler.stop();
  LOG.info("test tear down complete " + getName() + " "+ methodProfiler.timeLog());
  if (PROFILER_ON) {
    System.out.println(" \t" + methodProfiler.timeLog());
  }
}
