{
  if (shouldIgnore(cls)) {
    return null;
  }
  final Class<?> superclass=cls.getSuperclass();
  if (superclass != null && superclass.isEnum()) {
    return superclass;
  }
  if (ClassUtil.directlyImplements(cls,JavassistEnhanced.class)) {
    return getClass(cls.getSuperclass());
  }
  return cls;
}
