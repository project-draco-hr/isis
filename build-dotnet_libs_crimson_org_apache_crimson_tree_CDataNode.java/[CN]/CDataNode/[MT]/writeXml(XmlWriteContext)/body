{
  Writer out=context.getWriter();
  out.write("<![CDATA[");
  for (int i=0; i < data.length; i++) {
    char c=data[i];
    if (c == ']') {
      if ((i + 2) < data.length && data[i + 1] == ']' && data[i + 2] == '>') {
        out.write("]]]><![CDATA[");
        continue;
      }
    }
    out.write(c);
  }
  out.write("]]>");
}
