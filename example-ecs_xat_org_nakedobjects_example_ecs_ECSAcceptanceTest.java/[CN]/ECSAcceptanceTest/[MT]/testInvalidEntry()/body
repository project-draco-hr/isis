{
  TestObject creditCard=getTestClass(CreditCard.class.getName()).newInstance();
  creditCard.assertFieldEntryInvalid("expires","0/12");
  creditCard.assertFieldDoesNotContain("expires","0/12");
  try {
    creditCard.fieldEntry("expires","0/12");
    fail();
  }
 catch (  NakedAssertionFailedError expected) {
  }
}
