{
  if (!replaceRootElement && n instanceof Element && getDocumentElement() != null)   throw new DomEx(DomEx.HIERARCHY_REQUEST_ERR);
  if (!replaceRootElement && n instanceof DocumentType && getDoctype() != null)   throw new DomEx(DomEx.HIERARCHY_REQUEST_ERR);
  return super.insertBefore(n,refNode);
}
