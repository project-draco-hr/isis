{
  EntityRefNode retval=new EntityRefNode(entity);
  ((NodeBase)retval).setOwnerDocument((XmlDocument)this.getOwnerDocument());
  if (deep) {
    Node node;
    for (int i=0; (node=item(i)) != null; i++) {
      node=node.cloneNode(true);
      retval.appendChild(node);
    }
  }
  return retval;
}
