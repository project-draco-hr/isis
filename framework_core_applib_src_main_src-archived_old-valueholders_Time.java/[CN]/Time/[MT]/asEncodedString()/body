{
  Calendar cal=calendarValue();
  if (cal == null) {
    return "NULL";
  }
 else {
    StringBuffer data=new StringBuffer(4);
    int hour=cal.get(Calendar.HOUR_OF_DAY);
    data.append((hour <= 9) ? "0" : "");
    data.append(hour);
    int minute=cal.get(Calendar.MINUTE);
    data.append((minute <= 9) ? "0" : "");
    data.append(minute);
    return data.toString();
  }
}
