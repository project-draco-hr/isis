{
  canvas.drawLine(x,baseline,x + 200 - 1,baseline,Color.GRAY);
  canvas.drawLine(x,baseline - midpoint,x + 200 - 1,baseline - midpoint,Color.RED);
  canvas.drawLine(x,baseline - ascent,x + 200 - 1,baseline - ascent,Color.LIGHT_GRAY);
  canvas.drawLine(x,baseline + descent,x + 200 - 1,baseline + descent,Color.YELLOW);
  canvas.drawText("12345 abcdefghijk ABCDEFG",x,baseline,Color.BLACK,Style.NORMAL);
}
