{
  if (getState() == State.CLOSED) {
    return;
  }
  if (LOG.isDebugEnabled()) {
    LOG.debug("closing " + this);
  }
  objectStore.close();
  adapterManager.close();
  servicesInjector.close();
  objectFactory.close();
  adapterFactory.close();
  oidGenerator.close();
  setState(State.CLOSED);
}
