{
  for (int i=0; i < fSuiteCombo.getItemCount(); i++) {
    if (suite.equals(fSuiteCombo.getItemAt(i))) {
      fSuiteCombo.removeItemAt(i);
      fSuiteCombo.insertItemAt(suite,0);
      fSuiteCombo.setSelectedIndex(0);
      return;
    }
  }
  fSuiteCombo.insertItemAt(suite,0);
  fSuiteCombo.setSelectedIndex(0);
  pruneHistory();
}
