{
  if (test != null) {
    ListModel failures=getFailures();
    for (int i=0; i < failures.getSize(); i++) {
      TestFailure failure=(TestFailure)failures.getElementAt(i);
      if (failure.failedTest() == test) {
        fFailureView.showFailure(failure);
        return;
      }
    }
  }
  fFailureView.clear();
}
