{
  BufferedWriter bw=new BufferedWriter(new FileWriter(getSettingsFile()));
  try {
    for (int i=0; i < fSuiteCombo.getItemCount(); i++) {
      String testsuite=fSuiteCombo.getItemAt(i).toString();
      bw.write(testsuite,0,testsuite.length());
      bw.newLine();
    }
  }
  finally {
    bw.close();
  }
}
