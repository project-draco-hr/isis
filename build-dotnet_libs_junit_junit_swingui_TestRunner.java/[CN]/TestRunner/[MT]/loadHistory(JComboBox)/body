{
  BufferedReader br=new BufferedReader(new FileReader(getSettingsFile()));
  int itemCount=0;
  try {
    String line;
    while ((line=br.readLine()) != null) {
      combo.addItem(line);
      itemCount++;
    }
    if (itemCount > 0)     combo.setSelectedIndex(0);
  }
  finally {
    br.close();
  }
}
