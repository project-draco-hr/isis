{
  ImperativeFacet imperativeFacet=ImperativeFacetUtils.getImperativeFacet(facet);
  if (imperativeFacet == null) {
    return false;
  }
  List<Method> methods=imperativeFacet.getMethods();
  for (  Method method : methods) {
    if (method.equals(this.method)) {
      return true;
    }
  }
  return false;
}
