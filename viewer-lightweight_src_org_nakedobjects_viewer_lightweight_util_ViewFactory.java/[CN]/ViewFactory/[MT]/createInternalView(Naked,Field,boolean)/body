{
  if (forObject == null) {
    View view=create(emptyFieldProtoype,forObject,asField);
    LOG.debug(view + " created for " + forObject);
    return (InternalView)view;
  }
  if (iconized) {
    return createFieldView(forObject,asField);
  }
 else {
    return createOpenView(forObject,asField);
  }
}
