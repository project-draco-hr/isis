{
@edu.umd.cs.findbugs.annotations.SuppressWarnings("UMAC_UNCALLABLE_METHOD_OF_ANONYMOUS_CLASS") class Customer {
    @Disabled(When.NEVER) public void someAction(){
    }
  }
  final Method actionMethod=findMethod(Customer.class,"someAction");
  facetFactory.process(Customer.class,actionMethod,methodRemover,facetHolder);
  final Facet facet=facetHolder.getFacet(DisabledFacet.class);
  final DisabledFacetAbstract disabledFacetAbstract=(DisabledFacetAbstract)facet;
  assertEquals(org.apache.isis.metamodel.facets.When.NEVER,disabledFacetAbstract.value());
}
