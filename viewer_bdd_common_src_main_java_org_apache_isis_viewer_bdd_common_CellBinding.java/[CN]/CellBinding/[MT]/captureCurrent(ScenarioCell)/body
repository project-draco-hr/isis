{
  final ScenarioCell previousCell=getCurrentCell();
  setCurrentCell(cell);
  boolean shouldDitto=StringUtils.isNullOrEmpty(cell.getText());
  boolean canDitto=isDitto() && previousCell != null;
  if (shouldDitto && canDitto) {
    ditto(previousCell);
    dittoed=true;
  }
 else {
    dittoed=false;
  }
}
