{
  int index;
  if (field == null) {
    index=next++;
  }
 else {
    index=Integer.valueOf(field).intValue() - 1;
  }
  if (index < 0 || index >= parameters.length) {
    throw new ScimpiException("Parameter numbers should be between 1 and " + parameters.length + ": "+ index);
  }
  parameters[index]=value;
}
