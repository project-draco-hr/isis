{
  final Class<?> cls=processClassContext.getCls();
  final DomainObject domainObject=Annotations.getAnnotation(cls,DomainObject.class);
  final FacetHolder facetHolder=processClassContext.getFacetHolder();
  final AutoComplete annotation=Annotations.getAnnotation(processClassContext.getCls(),AutoComplete.class);
  Facet facet=autoCompleteValidator.flagIfPresent(AutoCompleteFacetForAutoCompleteAnnotation.create(annotation,facetHolder,servicesInjector));
  if (facet == null) {
    facet=AutoCompleteFacetForDomainObjectAnnotation.create(domainObject,facetHolder,servicesInjector);
  }
  FacetUtil.addFacet(facet);
}
