{
  init();
  final ObjectAdapter objectAdapter=getObjectAdapter(oidStr);
  final TypedReprBuilderFactory reprBuilderBuilder=builderFactoryRegistry.find(RepresentationType.DOMAIN_OBJECT);
  final DomainObjectReprBuilder repBuilder=reprBuilderBuilder.newBuilder(getResourceContext(),ObjectAdapter.class);
  repBuilder.with(objectAdapter);
  ResponseBuilder respBuilder=responseOfOk(RepresentationType.DOMAIN_OBJECT,Caching.NONE,repBuilder);
  Version version=objectAdapter.getVersion();
  if (version != null && version.getTime() != null) {
    respBuilder.tag(ETAG_FORMAT.format(version.getTime()));
  }
  return respBuilder.build();
}
