{
  final ObjectAdapter objectAdapter=getObjectAdapter(oidStr);
  final OneToManyAssociation collection=getCollectionThatIsVisibleAndUsable(objectAdapter,collectionId,Intent.ACCESS);
  final ObjectCollectionReprBuilder builder=ObjectCollectionReprBuilder.newBuilder(getResourceContext(),objectAdapter,collection);
  return Response.status(HttpStatusCode.OK.getJaxrsStatusType()).entity(jsonFor(builder)).type(MediaType.APPLICATION_JSON_TYPE).build();
}
