{
  init();
  final ObjectAdapter objectAdapter=getObjectAdapter(oidStr);
  final OneToOneAssociation property=getPropertyThatIsVisibleAndUsable(objectAdapter,propertyId,Intent.ACCESS);
  ResourceContext resourceContext=getResourceContext();
  final ObjectPropertyRepBuilder builder=ObjectPropertyRepBuilder.newBuilder(resourceContext,objectAdapter,property);
  return responseOfOk(RepresentationType.OBJECT_PROPERTY,Caching.NONE,builder).build();
}
