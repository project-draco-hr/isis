{
  for (  T o1 : getObjectsWithSameValue()) {
    for (    T o2 : getObjectsWithSameValue()) {
      for (      Object o3 : getObjectsWithSameValue()) {
        assertThat(o1.equals(o2),is(true));
        assertThat(o2.equals(o3),is(true));
        assertThat(o1.equals(o3),is(true));
      }
    }
  }
}
