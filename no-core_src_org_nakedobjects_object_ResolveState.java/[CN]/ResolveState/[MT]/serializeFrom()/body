{
  if (this == RESOLVED) {
    return SERIALIZING_RESOLVED;
  }
 else   if (this == PART_RESOLVED) {
    return SERIALIZING_PART_RESOLVED;
  }
 else   if (this == GHOST) {
    return SERIALIZING_GHOST;
  }
 else   if (this == TRANSIENT) {
    return SERIALIZING_TRANSIENT;
  }
 else {
    return null;
  }
}
