{
  InfoDebugFrame f=new InfoDebugFrame();
  Naked object=view.getContent().getNaked();
  f.setInfo(new DebugInfo[]{new DebugObjectDetail(object),new DebugObjectGraph(object),new DebugObjectSpecification(object),new DebugViewStructure(view),new DebugContent(view),new DebugDrawing(view),new DebugView(view)});
  f.show(at.getX() + 50,at.getY() + 6);
}
