{
  SnapshotWriter s;
  try {
    s=new SnapshotWriter(directoryPath,fileName,extension,message);
    s.appendLog(details);
    s.close();
  }
 catch (  FileNotFoundException e) {
    LOG.info("failed to open log file",e);
  }
catch (  IOException e) {
    LOG.info("failed to write log file",e);
  }
}
