{
  if (!this.overlaps(arg)) {
    throw new IllegalArgumentException("No overlap");
  }
 else {
    if (this.endsAfter(arg)) {
      getStart().setValue(arg.getEndDate());
      getStart().add(0,0,1);
    }
 else {
      getStart().setValue(getEndDate());
      getStart().add(0,0,1);
      getEndDate().setValue(arg.getEndDate());
    }
  }
}
