{
  super.draw(canvas);
  Bounds r=getBounds();
  if (getParent() == null) {
    canvas.drawRectangle(0,0,r.width - 1,r.height - 1,Style.IN_FOREGROUND);
  }
  if (icon == null) {
    String name=getObject().getIconName();
    icon=ImageFactory.getImageFactory().createIcon(name,ICON_SIZE,null);
  }
  canvas.drawIcon(icon,HPADDING,VPADDING);
}
