{
  db=new BerkleyDb();
  db.open();
  isDataLoaded=db.containsData();
  byte[] data=db.read(SERIAL_NUMBER);
  long loadSerialNumber;
  if (data == null) {
    loadSerialNumber=1;
  }
 else {
    loadSerialNumber=Long.valueOf(new String(data));
  }
  oidGenerator=new SimpleOidGenerator(loadSerialNumber);
  db.close();
}
