{
  final PropertyOrCollectionAccessorFacet getterFacet=holder.getFacet(PropertyOrCollectionAccessorFacet.class);
  if (getterFacet == null) {
    return null;
  }
  final Class<? extends PropertyInteractionEvent<?,?>> interaction=property.interaction();
  if (interaction == null) {
    return null;
  }
  return new PropertyInteractionFacetForPropertyAnnotation(interaction,getterFacet,servicesInjector,specificationLoader,holder);
}
