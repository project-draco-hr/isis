{
@edu.umd.cs.findbugs.annotations.SuppressWarnings("UMAC_UNCALLABLE_METHOD_OF_ANONYMOUS_CLASS") class Customer implements Iterable {
    public void someAction(){
    }
    @Override public Iterator iterator(){
      return null;
    }
  }
  final Method iteratorMethod=findMethod(Customer.class,"iterator");
  assertTrue(facetFactory.recognizes(iteratorMethod));
}
