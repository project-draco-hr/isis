{
  try {
    AbstractNakedValue clone=(AbstractNakedValue)getClass().newInstance();
    clone.restoreString(this.saveString());
    return clone;
  }
 catch (  InstantiationException e) {
    throw new NakedObjectRuntimeException(e);
  }
catch (  IllegalAccessException e) {
    throw new NakedObjectRuntimeException(e);
  }
}
