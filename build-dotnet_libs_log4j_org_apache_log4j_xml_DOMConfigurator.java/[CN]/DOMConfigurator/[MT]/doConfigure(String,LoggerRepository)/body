{
  FileInputStream fis=null;
  try {
    fis=new FileInputStream(filename);
    doConfigure(fis,repository);
  }
 catch (  IOException e) {
    LogLog.error("Could not open [" + filename + "].",e);
  }
 finally {
    if (fis != null) {
      try {
        fis.close();
      }
 catch (      java.io.IOException e) {
        LogLog.error("Could not close [" + filename + "].",e);
      }
    }
  }
}
