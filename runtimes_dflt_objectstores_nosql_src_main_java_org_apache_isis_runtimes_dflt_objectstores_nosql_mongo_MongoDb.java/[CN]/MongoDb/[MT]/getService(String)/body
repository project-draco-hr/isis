{
  final DBCollection services=db.getCollection("services");
  final DBObject object=services.findOne(new BasicDBObject().append("name",name));
  if (object == null) {
    return null;
  }
 else {
    final String id=(String)object.get("key");
    LOG.info("service found " + name + ":"+ id);
    return id;
  }
}
