{
  popup.keyPressed(KeyEvent.VK_DOWN,0);
  assertEquals(2,popup.getOption());
  popup.keyPressed(KeyEvent.VK_DOWN,0);
  assertEquals(3,popup.getOption());
  popup.keyPressed(KeyEvent.VK_DOWN,0);
  assertEquals(3,popup.getOption());
  popup.keyPressed(KeyEvent.VK_UP,0);
  assertEquals(2,popup.getOption());
  popup.keyPressed(KeyEvent.VK_UP,0);
  assertEquals(0,popup.getOption());
  popup.keyPressed(KeyEvent.VK_UP,0);
  assertEquals(0,popup.getOption());
}
