{
  int count=1;
  for (  final Oid oid : this) {
    final ObjectAdapter adapter=getAdapter(oid);
    debug.append(count++,5);
    debug.append(" '");
    debug.append(oid.toString(),15);
    debug.append("'    ");
    debug.appendln(adapter != null ? adapter.toString() : "(MISSING OBJECT ?!)");
  }
}
