{
  String[] cityNames={"Atlanta","Austin","Birmingham","Boston","Charleston","Chicago","Cleveland","Concord","Dallas","Denver","Des Moines","Fort Worth","Las Vegas","Manchester","Miami","Newark","New York","Paris","Portsmouth","Providence","Pittsburg","Richmond","Sacremento","San Fransico","Seattle","Tampa","Washington"};
  City[] cities=new City[cityNames.length];
  for (int i=0; i < cityNames.length; i++) {
    cities[i]=(City)createInstance(City.class);
    cities[i].setName(cityNames[i]);
  }
  boston=cities[1];
  newYork=cities[0];
  washington=cities[2];
}
