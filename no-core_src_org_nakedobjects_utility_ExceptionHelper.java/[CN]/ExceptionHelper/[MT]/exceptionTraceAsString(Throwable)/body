{
  ByteArrayOutputStream baos=null;
  try {
    baos=new ByteArrayOutputStream();
    exception.printStackTrace(new PrintStream(baos));
    return baos.toString();
  }
  finally {
    if (baos != null) {
      try {
        baos.close();
      }
 catch (      IOException ignore) {
      }
    }
  }
}
