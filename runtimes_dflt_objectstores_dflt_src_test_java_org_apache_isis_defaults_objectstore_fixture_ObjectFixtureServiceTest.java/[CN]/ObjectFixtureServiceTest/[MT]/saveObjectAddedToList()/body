{
  Person person=new Person();
  person.setName("Fred Smith");
  person.setDate(new Date(110,2,8,13,32));
  Movie movie=new Movie();
  movie.setDirector(person);
  movie.addToRoles(new Role());
  movie.addToRoles(new Role());
