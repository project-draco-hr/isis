{
  if (level > 3) {
    info.appendln("...");
  }
 else {
    info.blankLine();
    if (object instanceof NakedCollection) {
      collectionGraph((NakedCollection)object,level,ignoreObjects,info);
    }
 else     if (object instanceof NakedObject) {
      objectGraph((NakedObject)object,level,ignoreObjects,info);
    }
 else {
    }
  }
}
