{
  final String name=object.getIconName();
  final String imageName=(String)images.get(name);
  if (imageName != null) {
    return imageName;
  }
 else {
    final String imageFile=imageFile(name,contextPath);
    if (imageFile != null) {
      return imageFile;
    }
 else {
      return findImage(object.getSpecification(),contextPath);
    }
  }
}
