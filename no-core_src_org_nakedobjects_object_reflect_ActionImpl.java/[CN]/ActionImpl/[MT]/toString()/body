{
  StringBuffer sb=new StringBuffer();
  sb.append("Action [");
  sb.append(super.toString());
  sb.append(",type=");
  sb.append(getActionType());
  sb.append(",returns=");
  sb.append(getReturnType());
  sb.append(",parameters={");
  for (int i=0; i < parameters().length; i++) {
    if (i > 0) {
      sb.append(",");
    }
    sb.append(parameters()[i]);
  }
  sb.append("}]");
  return sb.toString();
}
