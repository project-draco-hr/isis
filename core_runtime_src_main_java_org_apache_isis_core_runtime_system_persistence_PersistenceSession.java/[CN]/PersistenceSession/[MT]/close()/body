{
  if (getState() == State.CLOSED) {
    return;
  }
  if (LOG.isDebugEnabled()) {
    LOG.debug("closing " + this);
  }
  if (eventBusService != null) {
    eventBusService.close();
  }
  try {
    objectStore.close();
  }
 catch (  RuntimeException ex) {
  }
  try {
    adapterManager.close();
  }
 catch (  RuntimeException ex) {
  }
  try {
    objectFactory.close();
  }
 catch (  RuntimeException ex) {
  }
  setState(State.CLOSED);
}
