{
  final Oid adapterOid=adapter.getOid();
  final ObjectAdapter adapterAccordingToOidAdapterMap=oidAdapterMap.getAdapter(adapterOid);
  ensureThatArg(adapter,is(adapterAccordingToOidAdapterMap),"mismatch in OidAdapter map: " + "adapter's Oid: " + adapterOid + ", "+ "provided adapter's OID: "+ adapter.getOid()+ "; "+ "map's adapter's Oid: "+ adapterAccordingToOidAdapterMap.getOid());
}
