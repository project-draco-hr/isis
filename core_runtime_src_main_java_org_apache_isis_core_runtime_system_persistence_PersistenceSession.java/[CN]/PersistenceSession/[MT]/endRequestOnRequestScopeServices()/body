{
  for (  final Object service : servicesInjector.getRegisteredServices()) {
    if (service instanceof RequestScopedService) {
      ((RequestScopedService)service).__isis_preDestroy();
    }
  }
  for (  final Object service : servicesInjector.getRegisteredServices()) {
    if (service instanceof RequestScopedService) {
      ((RequestScopedService)service).__isis_endRequest();
    }
  }
}
