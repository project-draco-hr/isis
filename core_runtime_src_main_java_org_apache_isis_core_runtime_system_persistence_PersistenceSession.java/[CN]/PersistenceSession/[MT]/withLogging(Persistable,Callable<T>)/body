{
  if (LOG.isDebugEnabled()) {
    LOG.debug(logString(LoggingLocation.ENTRY,pojo));
  }
  try {
    return runnable.call();
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
 finally {
    if (LOG.isDebugEnabled()) {
      LOG.debug(logString(LoggingLocation.EXIT,pojo));
    }
  }
}
