{
  if (this.levelMin != null) {
    if (event.getLevel().isGreaterOrEqual(levelMin) == false) {
      return Filter.DENY;
    }
  }
  if (this.levelMax != null) {
    if (event.getLevel().toInt() > levelMax.toInt()) {
      return Filter.DENY;
    }
  }
  if (acceptOnMatch) {
    return Filter.ACCEPT;
  }
 else {
    return Filter.NEUTRAL;
  }
}
