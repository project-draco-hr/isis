{
  Object object=transactionalObjects.get(oid);
  if (object == null) {
    NakedObject proxy=(NakedObject)original.getSpecification().acquireInstance();
    proxy.setupOid(original.getOid());
    loaded(proxy);
    proxy.setResolved();
    copyToProxy(original,proxy);
    object=proxy;
  }
  return (NakedObject)object;
}
