{
  if (domainServiceFacet.getNatureOfService() == NatureOfService.VIEW_REST_ONLY) {
    return true;
  }
  if (visibility.isPublic()) {
    return false;
  }
  return domainServiceFacet.getNatureOfService() == NatureOfService.VIEW_MENU_ONLY || domainServiceFacet.getNatureOfService() == NatureOfService.VIEW;
}
