{
  assertEquals("10:40",10 * 3600 + 40 * 60,t.longValue());
  t.parseUserEntry("+1");
  assertEquals("11:40",11 * 3600 + 40 * 60,t.longValue());
  t.parseUserEntry("+1");
  assertEquals("12:40",12 * 3600 + 40 * 60,t.longValue());
  t.parseUserEntry("+22");
  assertEquals("10:40",10 * 3600 + 40 * 60,t.longValue());
}
