{
  List<Vfs.UrlType> urlTypes=getUrlTypes();
  Vfs.setDefaultURLTypes(urlTypes);
  ConfigurationBuilder configuration=ConfigurationBuilder.build(ClasspathHelper.forClassLoader(Thread.currentThread().getContextClassLoader()),ClasspathHelper.forClass(Object.class),new SubTypesScanner(false));
  final Reflections reflections=new Reflections(configuration);
  return reflections.getSubTypesOf(type);
}
