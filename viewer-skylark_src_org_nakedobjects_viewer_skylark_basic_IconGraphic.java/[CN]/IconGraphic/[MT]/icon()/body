{
  final String iconName=content.getIconName();
  if (icon != null && (iconName == null || iconName.equals(lastIconName))) {
    return icon;
  }
  lastIconName=iconName;
  if (iconName != null) {
    final Image loadIcon=loadIcon(iconName);
    if (loadIcon != null) {
      icon=loadIcon;
      return loadIcon;
    }
  }
  icon=content.getIconPicture(iconHeight);
  return icon;
}
