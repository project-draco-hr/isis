{
  int y=baseline - getBaseline();
  if (AbstractView.debug) {
    canvas.drawDebugOutline(new Bounds(new Location(x,y),getSize()),getBaseline(),Color.DEBUG_DRAW_BOUNDS);
  }
  Image icon=icon();
  if (icon == null) {
    canvas.drawSolidOval(x,y,iconHeight,iconHeight,Style.PRIMARY3);
  }
 else {
    canvas.drawIcon(icon,x,y);
  }
}
