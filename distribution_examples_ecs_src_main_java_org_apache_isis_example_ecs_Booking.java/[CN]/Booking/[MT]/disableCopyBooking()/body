{
  int sets=0;
  sets+=((getCustomer() != null) ? 1 : 0);
  sets+=((getPickUp() != null) ? 1 : 0);
  sets+=((getDropOff() != null) ? 1 : 0);
  sets+=((getPaymentMethod() != null) ? 1 : 0);
  sets+=((getContactTelephone() != null) ? 1 : 0);
  return sets >= 3 ? null : "Not enough detail present to copy booking";
}
