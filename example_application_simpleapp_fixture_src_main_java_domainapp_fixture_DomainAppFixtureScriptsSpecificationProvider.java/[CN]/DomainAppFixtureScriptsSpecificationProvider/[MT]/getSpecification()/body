{
  return FixtureScriptsSpecification.builder(DomainAppFixtureScriptsSpecificationProvider.class).with(FixtureScripts.MultipleExecutionStrategy.EXECUTE).withRunScriptDefault(RecreateSimpleObjects.class).withRunScriptDropDown(FixtureScriptsSpecification.DropDownPolicy.CHOICES).withRecreate(RecreateSimpleObjects.class).build();
}
