{
  final int pos=serviceName.indexOf(DELIMITER);
  if (pos == 0) {
    return null;
  }
  final String type;
  if (pos != -1) {
    type=serviceName.substring(0,pos);
  }
 else {
    type=serviceName;
  }
  final Class<?> cls=loadClass(type);
  if (cls.isAnonymousClass()) {
    return null;
  }
  return serviceInstantiator.createInstance(cls);
}
