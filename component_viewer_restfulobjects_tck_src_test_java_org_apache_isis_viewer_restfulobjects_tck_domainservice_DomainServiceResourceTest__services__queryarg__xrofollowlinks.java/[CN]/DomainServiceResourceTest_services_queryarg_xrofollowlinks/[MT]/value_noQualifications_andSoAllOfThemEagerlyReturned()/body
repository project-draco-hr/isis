{
  request=client.createRequest(RestfulHttpMethod.GET,"services").withArg(RequestParameter.FOLLOW_LINKS,"value[rel=" + Rel.SERVICE.getName() + "]");
  restfulResponse=request.executeT();
  repr=restfulResponse.getEntity();
  request=client.createRequest(RestfulHttpMethod.GET,"services").withArg(RequestParameter.FOLLOW_LINKS,"value");
  restfulResponse=request.executeT();
  repr=restfulResponse.getEntity();
  for (  final LinkRepresentation link : repr.getValue().arrayIterable(LinkRepresentation.class)) {
    assertThat(link,isLink(client).rel(containsString(Rel.SERVICE.getName())).href(containsString(":39393/")).httpMethod(RestfulHttpMethod.GET).type(RepresentationType.DOMAIN_OBJECT.getMediaType()).value(is(not(nullValue(JsonRepresentation.class)))));
  }
}
