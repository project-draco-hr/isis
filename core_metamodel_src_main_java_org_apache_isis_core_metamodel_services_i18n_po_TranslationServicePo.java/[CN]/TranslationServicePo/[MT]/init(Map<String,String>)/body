{
  final String deploymentType=config.get(KEY_DEPLOYMENT_TYPE);
  prototype=deploymentType == null || deploymentType.toLowerCase().contains("prototype") || deploymentType.toLowerCase().contains("test");
  String translationMode=config.get(KEY_PO_MODE);
  final boolean forceRead=translationMode != null && ("read".equalsIgnoreCase(translationMode) || "reader".equalsIgnoreCase(translationMode));
  if (!prototype || forceRead) {
    final PoReader poReader=new PoReader(this);
    poReader.init(config);
    po=poReader;
  }
}
