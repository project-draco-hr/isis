{
  final StringBuffer sql=new StringBuffer();
  sql.append(versionColumn);
  sql.append(" bigint");
  sql.append(",");
  sql.append(lastActivityUserColumn);
  sql.append(" varchar(32)");
  sql.append(",");
  sql.append(lastActivityDateColumn);
  sql.append(" " + JdbcConnector.TYPE_TIMESTAMP());
  return sql.toString();
}
