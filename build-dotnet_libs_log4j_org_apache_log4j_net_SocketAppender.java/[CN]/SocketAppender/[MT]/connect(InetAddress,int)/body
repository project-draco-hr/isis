{
  if (this.address == null)   return;
  try {
    cleanUp();
    oos=new ObjectOutputStream(new Socket(address,port).getOutputStream());
  }
 catch (  IOException e) {
    LogLog.error("Could not connect to remote log4j server at [" + address.getHostName() + "]. We will try again later.",e);
    fireConnector();
  }
}
