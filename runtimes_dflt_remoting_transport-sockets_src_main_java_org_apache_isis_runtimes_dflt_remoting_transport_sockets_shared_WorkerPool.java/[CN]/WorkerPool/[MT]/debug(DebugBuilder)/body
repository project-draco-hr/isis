{
  debug.appendln("available",available);
  debug.appendln();
  debug.appendln();
  for (int i=0; i < available.size(); i++) {
    final Worker worker=(Worker)available.elementAt(i);
    debug.appendln(i + 1 + ". "+ worker.toString());
    debug.indent();
    worker.debug(debug);
    debug.unindent();
    debug.appendln();
  }
}
