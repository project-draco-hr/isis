{
  final DomainObjectInvocationHandler<T> invocationHandler=new DomainObjectInvocationHandler<T>(domainObject,embeddedViewer,mode,authenticationSessionProvider,specificationLookup,adapterManager,objectPersistor);
  final CgLibProxy<T> cglibProxy=new CgLibProxy<T>(invocationHandler);
  return cglibProxy.proxy();
}
