{
  target.prependJavaScript("Wicket.Window.unloadConfirmation = false;");
  super.show(target);
  StringBuilder builder=new StringBuilder();
  builder.append("window.setTimeout(" + "function() {\n " + "  var ww = Wicket.Window.get();\n "+ "  ww.autoSizeWindow();\n "+ "  $('.actionPanel .myBlockContainer').css('margin', '19px');\n "+ "}\n, 0);\n");
  target.appendJavaScript(builder.toString());
}
