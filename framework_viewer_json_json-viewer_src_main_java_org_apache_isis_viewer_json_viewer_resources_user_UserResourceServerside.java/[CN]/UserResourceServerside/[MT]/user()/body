{
  init();
  ResourceContext resourceContext=getResourceContext();
  UserRepBuilder builder=UserRepBuilder.newBuilder(resourceContext).withAuthenticationSession(getAuthenticationSession());
  return Response.ok().entity(jsonFrom(builder)).cacheControl(CACHE_ONE_HOUR).header(RestfulResponse.Header.X_REPRESENTATION_TYPE.getName(),RepresentationType.USER.getName()).type(MediaType.APPLICATION_JSON_TYPE).build();
}
