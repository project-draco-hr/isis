{
  final Timestamp fromTs=toTimestampStartOfDayWithOffset(from,0);
  final Timestamp toTs=toTimestampStartOfDayWithOffset(to,1);
  final Query<CommandJdo> query;
  if (from != null) {
    if (to != null) {
      query=new QueryDefault<CommandJdo>(CommandJdo.class,"findByTimestampBetween","from",fromTs,"to",toTs);
    }
 else {
      query=new QueryDefault<CommandJdo>(CommandJdo.class,"findByTimestampAfter","from",fromTs);
    }
  }
 else {
    if (to != null) {
      query=new QueryDefault<CommandJdo>(CommandJdo.class,"findByTimestampBefore","to",toTs);
    }
 else {
      query=new QueryDefault<CommandJdo>(CommandJdo.class,"find");
    }
  }
  return allMatches(query);
}
