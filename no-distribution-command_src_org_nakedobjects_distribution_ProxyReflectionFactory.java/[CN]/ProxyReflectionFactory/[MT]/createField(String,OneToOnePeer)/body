{
  OneToOnePeer oneToOneDelegate=new ProxyOneToOneAssociation(local,connection,objectDataFactory);
  oneToOneDelegate=new OneToOneHelp(oneToOneDelegate,helpManger);
  OneToOneAssociation association=new OneToOneAssociation(className,oneToOneDelegate.getName(),oneToOneDelegate.getType(),oneToOneDelegate);
  return association;
}
