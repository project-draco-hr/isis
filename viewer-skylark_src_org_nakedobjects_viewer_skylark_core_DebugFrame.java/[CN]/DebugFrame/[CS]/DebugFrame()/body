{
  frames.addElement(this);
  setLayout(new BorderLayout(7,7));
  addWindowListener(new WindowAdapter(){
    DebugFrame frame=DebugFrame.this;
    public void windowClosing(    WindowEvent e){
      closeDialog();
    }
  }
);
  TextArea area=new TextArea("",40,80,TextArea.SCROLLBARS_BOTH);
  area.setForeground(Style.BLACK.getAwtColor());
  Font font=Style.DEBUG.getAwtFont();
  area.setFont(font);
  add("Center",area);
  field=area;
  Panel buttons=new Panel();
  buttons.setLayout(new FlowLayout());
  add(buttons,BorderLayout.SOUTH);
  Button b=new java.awt.Button("Refresh");
  b.setFont(font);
  buttons.add(b);
  b.addActionListener(new java.awt.event.ActionListener(){
    public void actionPerformed(    ActionEvent e){
      refresh();
    }
  }
);
  b=new java.awt.Button("Print...");
  b.setFont(font);
  b.setEnabled(false);
  buttons.add(b);
  b.addActionListener(new java.awt.event.ActionListener(){
    public void actionPerformed(    ActionEvent e){
    }
  }
);
  b=new java.awt.Button("Save...");
  b.setFont(font);
  b.setEnabled(false);
  buttons.add(b);
  b.addActionListener(new java.awt.event.ActionListener(){
    public void actionPerformed(    ActionEvent e){
    }
  }
);
  b=new java.awt.Button("Close");
  b.setFont(font);
  buttons.add(b);
  b.addActionListener(new java.awt.event.ActionListener(){
    public void actionPerformed(    ActionEvent e){
      closeDialog();
    }
  }
);
}
