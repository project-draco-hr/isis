{
  char c=getc();
  if (c == '?' || c == '+' || c == '*') {
    strTmp.append(c);
    if (original == null)     return null;
    if (original.type == 0) {
      original.type=c;
      return original;
    }
    return newContentModel(c,original);
  }
 else {
    ungetc();
    return original;
  }
}
