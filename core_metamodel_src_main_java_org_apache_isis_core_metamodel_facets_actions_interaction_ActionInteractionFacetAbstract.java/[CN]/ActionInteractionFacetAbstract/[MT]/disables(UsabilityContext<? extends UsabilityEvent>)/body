{
  if (!interactionHelper.hasEventBusService()) {
    return null;
  }
  currentInteraction.set(null);
  final ActionInteractionEvent<?> event=interactionHelper.postEventForAction(eventType(),null,null,AbstractInteractionEvent.Phase.DISABLE,getIdentified(),ic.getTarget(),null);
  if (event != null && event.isDisabled()) {
    return event.getDisabledReason();
  }
  return null;
}
