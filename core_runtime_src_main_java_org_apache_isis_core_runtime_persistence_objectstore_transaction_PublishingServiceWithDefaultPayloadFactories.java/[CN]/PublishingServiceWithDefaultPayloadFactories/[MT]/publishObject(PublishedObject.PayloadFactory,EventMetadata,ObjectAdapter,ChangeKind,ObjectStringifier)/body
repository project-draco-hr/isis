{
  final PublishedObject.PayloadFactory payloadFactoryToUse=payloadFactoryIfAny != null ? payloadFactoryIfAny : defaultObjectPayloadFactory;
  final EventPayload payload=payloadFactoryToUse.payloadFor(ObjectAdapterUtils.unwrapObject(undeletedElseEmpty(changedAdapter)),changeKind);
  payload.withStringifier(stringifier);
  publishingService.publish(metadata,payload);
}
