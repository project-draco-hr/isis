{
  if (!interactionHelper.hasEventBusService()) {
    return null;
  }
  currentInteraction.set(null);
  final ProposedHolder catc=(ProposedHolder)ic;
  final Object proposed=catc.getProposed().getObject();
  final CollectionDomainEvent<?,?> event=interactionHelper.postEventForCollection(eventType(),null,AbstractDomainEvent.Phase.VALIDATE,getIdentified(),ic.getTarget(),CollectionDomainEvent.Of.ADD_TO,proposed);
  if (event != null && event.isInvalid()) {
    return event.getInvalidityReason();
  }
  currentInteraction.set(event);
  return null;
}
