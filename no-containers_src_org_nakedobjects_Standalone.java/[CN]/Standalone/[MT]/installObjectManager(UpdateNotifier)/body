{
  NakedObjectStore objectStore=(NakedObjectStore)ComponentLoader.loadComponent(OBJECT_STORE,TransientObjectStore.class,NakedObjectStore.class);
  OidGenerator oidGenerator=(OidGenerator)ComponentLoader.loadComponent("oidgenerator",SimpleOidGenerator.class,OidGenerator.class);
  ObjectFactory objectFactory=(ObjectFactory)ComponentLoader.loadComponent("object-factory",ObjectFactory.class);
  LocalObjectManager objectManager=new LocalObjectManager(objectStore,updateNotifier,oidGenerator,objectFactory);
  objectManager.init();
  return objectManager;
}
