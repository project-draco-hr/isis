{
  System.err.println("usage:");
  System.err.println(" " + programName + " ");
  for (  Object optObj : options.getOptions()) {
    Option option=(Option)optObj;
    System.err.println("       -" + option.getOpt() + "  "+ (option.isRequired() ? "[required] " : "")+ option.getDescription());
  }
}
