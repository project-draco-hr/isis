{
  try {
    Hashtable lookup=new Hashtable();
    ResultSet rs=select("select " + key + ","+ value+ " from "+ table);
    while (rs.next()) {
      lookup.put(rs.getString(key),rs.getString(value));
    }
    return new Lookup(lookup);
  }
 catch (  SQLException e) {
    throw new ObjectStoreException(e);
  }
}
