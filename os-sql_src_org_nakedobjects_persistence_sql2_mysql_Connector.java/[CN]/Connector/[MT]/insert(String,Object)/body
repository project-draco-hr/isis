{
  LOG.debug("SQL: " + sql);
  PreparedStatement statement;
  try {
    statement=connection.prepareStatement(sql);
    statement.executeUpdate();
    ResultSet rs=statement.getGeneratedKeys();
    if (rs.next()) {
      int id=rs.getInt(1);
    }
    statement.close();
  }
 catch (  SQLException e) {
    throw new ObjectStoreException("SQL error",e);
  }
}
