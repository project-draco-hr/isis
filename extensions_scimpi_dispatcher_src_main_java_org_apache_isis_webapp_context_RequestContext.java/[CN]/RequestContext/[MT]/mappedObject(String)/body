{
  if (id != null && id.equals("")) {
    return null;
  }
  if (id == null) {
    id=RESULT;
  }
  ObjectAdapter mappedObject=objectMapping.mappedObject(id);
  if (mappedObject instanceof ObjectAdapter) {
    IsisContext.getPersistenceSession().resolveImmediately((ObjectAdapter)mappedObject);
  }
  return mappedObject;
}
