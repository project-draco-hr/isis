{
  if (id == null || id.trim().equals("") || id.trim().equals("null")) {
    return null;
  }
  if (id.equals("collection")) {
    return collection;
  }
  ObjectAdapter object=mappedObject(id);
  if (object == null) {
    throw new ScimpiException("No object for " + id);
  }
 else {
    return object;
  }
}
