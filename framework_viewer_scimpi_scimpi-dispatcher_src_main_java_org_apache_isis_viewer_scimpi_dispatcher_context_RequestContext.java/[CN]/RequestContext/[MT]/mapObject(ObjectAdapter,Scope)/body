{
  if (object.isValue()) {
    return object.titleString();
  }
 else   if (scope == Scope.INTERACTION && object.representsTransient()) {
    return objectMapping.mapTransientObject(object);
  }
 else   if (object.getOid() != null) {
    return objectMapping.mapObject(object,scope);
  }
 else {
    collection=object;
    return "collection";
  }
}
