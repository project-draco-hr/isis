{
  if (!domainEventHelper.hasEventBusService()) {
    return null;
  }
  currentInteraction.set(null);
  final PropertyDomainEvent<?,?> event=domainEventHelper.postEventForProperty(eventType(),null,AbstractDomainEvent.Phase.DISABLE,getIdentified(),ic.getTarget(),null,null);
  if (event != null && event.isDisabled()) {
    return event.getDisabledReason();
  }
  return null;
}
