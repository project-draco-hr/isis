{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  Link other=(Link)obj;
  if (getHref() == null) {
    if (other.getHref() != null)     return false;
  }
 else   if (!getHref().equals(other.getHref()))   return false;
  if (getMethod() != other.getMethod())   return false;
  if (getRel() == null) {
    if (other.getRel() != null)     return false;
  }
 else   if (!getRel().equals(other.getRel()))   return false;
  return true;
}
