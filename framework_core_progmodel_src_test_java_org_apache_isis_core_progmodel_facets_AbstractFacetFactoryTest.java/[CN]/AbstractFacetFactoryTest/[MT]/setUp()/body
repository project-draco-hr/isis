{
  Logger.getRootLogger().setLevel(Level.OFF);
  super.setUp();
  BasicConfigurator.configure();
  reflector=new ProgrammableReflector();
  facetHolder=new FacetHolderImpl();
  facetedMethod=FacetedMethod.createProperty(Customer.class,"firstName");
