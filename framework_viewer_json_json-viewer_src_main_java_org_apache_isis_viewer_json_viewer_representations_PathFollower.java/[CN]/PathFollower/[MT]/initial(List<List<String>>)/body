{
  final Map<String,List<String>> map=Maps.newHashMap();
  for (  List<String> list : followLinks) {
    int size=list.size();
    if (size == 0) {
      continue;
    }
    String path=list.get(0);
    list.remove(0);
    map.put(path,list);
  }
  return new PathFollower(map,null,Mode.FINDING_PATH);
}
