{
  int flatIndex=0;
  final int height=flatIntArray[flatIndex++];
  final int width=flatIntArray[flatIndex++];
  final int[][] newImage=new int[height][width];
  for (int i=0; i < height; i++) {
    for (int j=0; j < width; j++) {
      newImage[i][j]=flatIntArray[flatIndex++];
    }
  }
  return newImage;
}
