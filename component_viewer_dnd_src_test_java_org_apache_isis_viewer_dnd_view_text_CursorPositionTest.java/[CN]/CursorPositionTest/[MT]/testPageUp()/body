{
  cursor.pageUp();
  assertPosition(10,3,cursor);
  cursor.pageUp();
  assertPosition(8,3,cursor);
  cursor.pageUp();
  assertPosition(6,3,cursor);
  cursor.pageUp();
  assertPosition(4,3,cursor);
  cursor.pageUp();
  assertPosition(2,3,cursor);
  cursor.pageUp();
  assertPosition(0,3,cursor);
  cursor.pageUp();
  assertPosition(0,3,cursor);
}
