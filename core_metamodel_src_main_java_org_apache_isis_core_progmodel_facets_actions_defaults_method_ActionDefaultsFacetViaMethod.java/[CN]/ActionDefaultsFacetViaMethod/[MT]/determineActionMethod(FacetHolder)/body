{
  Method method2;
  final Facet actionInvocationFacet=holder.getFacet(ActionInvocationFacet.class);
  if (actionInvocationFacet instanceof InteractionWithActionFacetAbstract) {
    final InteractionWithActionFacetAbstract facetViaMethod=(InteractionWithActionFacetAbstract)actionInvocationFacet;
    method2=facetViaMethod.getMethods().get(0);
  }
 else {
    method2=null;
  }
  return method2;
}
