{
  LOG.info("shutting down " + this);
  if (transaction != null) {
    try {
      abortTransaction();
    }
 catch (    Exception e2) {
      LOG.error("failure during abort",e2);
    }
  }
  objectsToBeSaved.shutdown();
  objectsToRefreshViewsFor.shutdown();
  persistAlgorithm.shutdown();
  objectStore.shutdown();
  objectStore=null;
  nakedClasses.clear();
}
