{
  if (content.isObject() && content.getNaked() != null) {
    NakedObject object=(NakedObject)content.getNaked();
    NakedObjectField[] fields=object.getVisibleFields();
    for (int i=0; i < fields.length; i++) {
      if (fields[i].isCollection()) {
        return true;
      }
    }
  }
  return false;
}
