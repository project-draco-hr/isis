{
  String lineNumbers;
  int startingLine=((TagNode)node).getStartingLineNumber() + 1;
  int endingLine=((TagNode)node).getStartingLineNumber() + 1;
  if (startingLine == endingLine) {
    lineNumbers="" + startingLine;
  }
 else {
    lineNumbers=startingLine + "-" + endingLine;
  }
  return lineNumbers;
}
