{
  int bl=getBaseline();
  icon.draw(canvas,1,bl);
  title.draw(canvas,icon.getSize().getWidth() + HPADDING,bl);
  int l=getLeft() - 1;
  canvas.drawLine(0,0,l,0,Style.SECONDARY2);
  canvas.drawLine(l,0,l,getSize().getHeight(),Style.SECONDARY2);
  l--;
  canvas.drawLine(l,0,l,getSize().getHeight(),Style.SECONDARY2);
  super.draw(canvas);
}
