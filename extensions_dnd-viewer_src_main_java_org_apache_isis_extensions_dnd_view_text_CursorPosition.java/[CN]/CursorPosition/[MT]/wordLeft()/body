{
  if (!((line == 0) && (character == 0))) {
    if (character == 0) {
      line--;
      end();
    }
    final String text=textContent.getText(line);
    do {
      character--;
    }
 while ((character >= 0) && (text.charAt(character) == ' '));
    while ((character >= 0) && (text.charAt(character) != ' ')) {
      character--;
    }
    character++;
  }
}
