{
  final StringBuffer sb=new StringBuffer();
  sb.append("Action [");
  sb.append(super.toString());
  sb.append(",type=");
  sb.append(getType());
  sb.append(",returns=");
  sb.append(getReturnType());
  sb.append(",parameters={");
  for (int i=0; i < getParameterCount(); i++) {
    if (i > 0) {
      sb.append(",");
    }
    sb.append(getParameters().get(i).getSpecification().getShortName());
  }
  sb.append("}]");
  return sb.toString();
}
