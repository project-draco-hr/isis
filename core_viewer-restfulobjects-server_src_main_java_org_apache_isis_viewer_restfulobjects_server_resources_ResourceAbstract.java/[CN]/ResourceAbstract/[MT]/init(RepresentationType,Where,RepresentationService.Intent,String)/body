{
  if (!getIsisSessionFactory().inSession()) {
    throw RestfulObjectsApplicationException.create(HttpStatusCode.UNAUTHORIZED);
  }
  if (getAuthenticationSession() == null) {
    throw RestfulObjectsApplicationException.create(HttpStatusCode.UNAUTHORIZED);
  }
  this.resourceContext=new ResourceContext(representationType,httpHeaders,providers,uriInfo,request,where,intent,urlUnencodedQueryString,httpServletRequest,httpServletResponse,securityContext,getConfiguration(),getServicesInjector(),InteractionInitiatedBy.USER);
}
