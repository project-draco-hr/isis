{
  if (hasMemberFacet(mutatorSpec.mutatorFacetType)) {
    JsonRepresentation arguments=mutatorArgs(mutatorSpec);
    JsonRepresentation mutatorLink=linkToBuilder.linkToMember(mutatorSpec.rel,memberType,objectMember,mutatorSpec.suffix).withHttpMethod(mutatorSpec.httpMethod).withArguments(arguments).build();
    getLinks().arrayAdd(mutatorLink);
  }
}
