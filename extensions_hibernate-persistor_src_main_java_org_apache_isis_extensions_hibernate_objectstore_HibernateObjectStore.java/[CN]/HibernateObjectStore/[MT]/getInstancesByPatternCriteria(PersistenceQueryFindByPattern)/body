{
  try {
    final boolean started=startHibernateTransaction();
    final Object pojoPattern=criteria.getPattern().getObject();
    final Criteria hibernateCriteria=HibernateUtil.getCurrentSession().createCriteria(pojoPattern.getClass()).add(Example.create(pojoPattern).excludeProperty("title"));
    final List<?> results=hibernateCriteria.list();
    final ObjectAdapter[] objects=loadObjects(HibernateUtil.getCurrentSession(),criteria.getSpecification(),results);
    commitHibernateTransaction(started);
    return objects;
  }
 catch (  final RuntimeException e) {
    HibernateUtil.rollbackTransaction();
    throw e;
  }
}
