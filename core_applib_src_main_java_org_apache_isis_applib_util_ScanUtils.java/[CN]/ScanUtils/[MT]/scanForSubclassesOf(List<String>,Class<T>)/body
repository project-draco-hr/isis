{
  final List<Class<? extends T>> classes=Lists.newArrayList();
  new FastClasspathScanner(packagePrefixList.toArray(new String[]{})).matchSubclassesOf(superClass,new SubclassMatchProcessor<T>(){
    @Override public void processMatch(    final Class<? extends T> matchingClass){
      classes.add(matchingClass);
    }
  }
).scan();
  return classes;
}
