{
  About about=action.getAbout(ClientSession.getSession(),object);
  if (about.canUse().isAllowed()) {
    String description=about.getDescription();
    if (action.hasReturn()) {
      description+=" returns a " + action.getReturnType();
    }
    return new Allow(description);
  }
 else {
    return about.canUse();
  }
}
