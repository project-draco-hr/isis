{
  if (properties == null) {
    return null;
  }
  String sortedBy=properties.getProperty("sortedBy");
  if (sortedBy == null) {
    return null;
  }
  final Class<?> sortedByClass=ClassUtil.forName(sortedBy);
  if (sortedByClass == Comparator.class) {
    return null;
  }
  return sortedByClass;
}
