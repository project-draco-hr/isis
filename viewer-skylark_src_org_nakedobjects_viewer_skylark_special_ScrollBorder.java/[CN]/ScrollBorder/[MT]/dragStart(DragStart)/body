{
  int area=adjust(drag);
  dragArea=area;
switch (area) {
case NORTH:
    throw new NotImplementedException();
case WEST:
  throw new NotImplementedException();
case CENTER:
throw new NotImplementedException();
case SOUTH:
return dragStartSouth(drag);
case EAST:
return dragStartEast(drag);
default :
return null;
}
}
