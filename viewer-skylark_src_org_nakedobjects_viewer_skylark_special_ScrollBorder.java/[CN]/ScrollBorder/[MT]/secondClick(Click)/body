{
  int area=adjust(click);
switch (area) {
case NORTH:
    topHeader.secondClick(click);
  break;
case WEST:
leftHeader.secondClick(click);
break;
case CENTER:
wrappedView.secondClick(click);
break;
case SOUTH:
secondClickSouth(click);
break;
case EAST:
secondClickEast(click);
break;
default :
break;
}
}
