{
  final CollectionInteractionEvent<?,?> existingEvent=currentInteraction.get();
  final CollectionInteractionEvent<?,?> event=interactionHelper.postEventForCollection(eventType(),existingEvent,AbstractInteractionEvent.Phase.DISABLE,getIdentified(),ic.getTarget(),CollectionInteractionEvent.Of.ACCESS,null);
  if (event == null || !event.isDisabled()) {
    currentInteraction.set(event);
    return null;
  }
 else {
    currentInteraction.set(null);
    return event.getDisabledReason();
  }
}
