{
  final CollectionInteractionEvent<?,?> existingEvent=currentInteraction.get();
  final ProposedHolder catc=(ProposedHolder)ic;
  final Object proposed=catc.getProposed().getObject();
  final CollectionInteractionEvent<?,?> event=interactionHelper.postEventForCollection(eventType(),existingEvent,AbstractInteractionEvent.Phase.VALIDATE,getIdentified(),ic.getTarget(),CollectionInteractionEvent.Of.ADD_TO,proposed);
  if (event == null || !event.isInvalid()) {
    currentInteraction.set(event);
    return null;
  }
 else {
    currentInteraction.set(null);
    return event.getInvalidityReason();
  }
}
