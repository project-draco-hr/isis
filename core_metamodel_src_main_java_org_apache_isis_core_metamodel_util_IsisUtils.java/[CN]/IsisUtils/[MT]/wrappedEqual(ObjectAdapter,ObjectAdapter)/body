{
  final boolean defined1=exists(adapter1);
  final boolean defined2=exists(adapter2);
  if (defined1 && !defined2) {
    return false;
  }
  if (!defined1 && defined2) {
    return false;
  }
  if (!defined1 && !defined2) {
    return true;
  }
  return adapter1.getObject().equals(adapter2.getObject());
}
