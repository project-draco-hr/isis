{
  super.viewMenuOptions(menuOptions);
  menuOptions.add(new AbstractUserAction("Open"){
    public void execute(    Workspace workspace,    View view,    Location at){
      openIcon();
    }
  }
);
  menuOptions.add(new AbstractUserAction("Close"){
    public void execute(    Workspace workspace,    View view,    Location at){
      closeIcon();
    }
  }
);
}
