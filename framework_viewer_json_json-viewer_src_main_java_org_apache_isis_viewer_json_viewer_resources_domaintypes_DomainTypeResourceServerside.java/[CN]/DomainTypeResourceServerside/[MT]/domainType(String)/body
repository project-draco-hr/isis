{
  init();
  final ObjectSpecification objectSpec=getSpecificationLoader().loadSpecification(domainType);
  final RendererFactory rendererFactory=rendererFactoryRegistry.find(RepresentationType.DOMAIN_TYPE);
  final DomainTypeReprRenderer renderer=(DomainTypeReprRenderer)rendererFactory.newRenderer(getResourceContext(),JsonRepresentation.newMap());
  renderer.with(objectSpec).includesSelf();
  return responseOfOk(Caching.ONE_DAY,renderer).build();
}
