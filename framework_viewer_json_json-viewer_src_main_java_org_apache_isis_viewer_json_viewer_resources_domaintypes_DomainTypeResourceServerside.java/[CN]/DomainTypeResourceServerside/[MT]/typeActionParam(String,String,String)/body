{
  init();
  JsonRepresentation representation=JsonRepresentation.newMap();
  representation.mapPut("self",LinkBuilder.newBuilder(getResourceContext(),"self",RepresentationType.TYPE_ACTION_PARAMETER,"domainTypes/%s/actions/%s/params/%s",domainType,actionId,paramName).build());
  return responseOfOk(RepresentationType.TYPE_COLLECTION,Caching.ONE_DAY,representation).build();
}
