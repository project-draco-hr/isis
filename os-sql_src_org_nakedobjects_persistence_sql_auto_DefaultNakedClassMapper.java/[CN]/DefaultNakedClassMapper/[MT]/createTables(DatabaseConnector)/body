{
  ValueMapperLookup mappers=ValueMapperLookup.getInstance();
  connector.update("create table " + table + " ("+ idColumn+ " "+ "INT NOT NULL UNIQUE, "+ nameColumn+ " "+ mappers.mapperFor(TextString.class).columnType()+ " UNIQUE, "+ reflectorColumn+ " "+ mappers.mapperFor(TextString.class).columnType()+ ")");
}
