{
  LOG.debug("saving naked class: " + cls);
  String statement="insert into " + table + " ("+ idColumn+ ", "+ nameColumn+ ", "+ reflectorColumn+ ") values ("+ primaryKey(cls.getOid())+ ",'"+ cls.getName().stringValue()+ "','"+ cls.getReflector().stringValue()+ "')";
  connector.update(statement);
  cls.setResolved();
}
