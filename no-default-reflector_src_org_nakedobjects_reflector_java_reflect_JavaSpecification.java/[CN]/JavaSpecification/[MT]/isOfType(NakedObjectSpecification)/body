{
  if (specification == this) {
    return true;
  }
 else {
    if (interfaces != null) {
      for (int i=0, len=interfaces.length; i < len; i++) {
        if (interfaces[i].isOfType(specification)) {
          return true;
        }
      }
    }
    if (superclass != null) {
      return superclass.isOfType(specification);
    }
  }
  return false;
}
