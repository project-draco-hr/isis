{
  Assert.assertNotNull(cls);
  NakedObjectSpecification spec=cache.get(cls);
  if (spec != null) {
    return spec;
  }
  if (cls.isPrimitive()) {
synchronized (cache) {
      spec=new PrimitiveSpecification(cls.getName());
      cache.cache(cls,spec);
    }
    return spec;
  }
 else {
    return load(cls);
  }
}
