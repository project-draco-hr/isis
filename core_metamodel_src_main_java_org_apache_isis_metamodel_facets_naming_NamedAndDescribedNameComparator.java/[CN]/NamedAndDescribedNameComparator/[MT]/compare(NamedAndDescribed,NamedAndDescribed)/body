{
  final String name1=o1.getName();
  final String name2=o2.getName();
  if (name1 == null && name2 == null) {
    return 0;
  }
  if (name1 == null && name2 != null) {
    return -1;
  }
  if (name1 != null && name2 == null) {
    return +1;
  }
  if (name1 != null && name2 != null) {
    return name1.compareTo(name2);
  }
  return 0;
}
