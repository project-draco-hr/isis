{
  String model=element.contentType;
  for (int index=8; (index=model.indexOf(type,index + 1)) >= 9; ) {
    char c;
    if (model.charAt(index - 1) != '|')     continue;
    c=model.charAt(index + type.length());
    if (c == '|' || c == ')')     return;
  }
  error("V-034",new Object[]{element.name,type,model});
}
