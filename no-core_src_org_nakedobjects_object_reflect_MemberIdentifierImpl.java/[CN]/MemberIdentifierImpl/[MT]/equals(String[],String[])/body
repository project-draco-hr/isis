{
  if (a == null && b == null) {
    return true;
  }
  if (a == null && b != null) {
    return false;
  }
  if (a != null && b == null) {
    return false;
  }
  if (a.length != b.length) {
    return false;
  }
  for (int i=0; i < b.length; i++) {
    if (!a[i].equals(b[i])) {
      return false;
    }
  }
  return true;
}
