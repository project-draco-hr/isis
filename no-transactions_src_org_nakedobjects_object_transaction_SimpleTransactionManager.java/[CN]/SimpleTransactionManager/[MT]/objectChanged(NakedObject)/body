{
  Transaction t=getTransaction();
  if (t == null) {
    log("save object " + object);
    startTransaction();
    objectChanged(object);
    endTransaction();
    return;
  }
  if (!t.isInUpdate()) {
    log("schedule save - " + object);
    t.prepareSave(object);
  }
 else {
    LOG.debug("  update of public object " + object);
  }
}
