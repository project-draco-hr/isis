{
  FileWriter out=null;
  try {
    File file=file(true);
    out=new FileWriter(file);
    persistor.save(objects,out);
  }
 catch (  IOException e) {
    throw new IsisException(e);
  }
 finally {
    if (out != null) {
      try {
        out.close();
      }
 catch (      IOException e) {
        throw new IsisException(e);
      }
    }
  }
}
