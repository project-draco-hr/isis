{
  OidDto argValue;
  if (reference != null) {
    argValue=new OidDto();
    argValue.setObjectType(reference.getObjectType());
    argValue.setObjectState(bookmarkObjectStateOf(reference));
    argValue.setObjectIdentifier(reference.getIdentifier());
  }
 else {
    argValue=null;
  }
  return argValue;
}
