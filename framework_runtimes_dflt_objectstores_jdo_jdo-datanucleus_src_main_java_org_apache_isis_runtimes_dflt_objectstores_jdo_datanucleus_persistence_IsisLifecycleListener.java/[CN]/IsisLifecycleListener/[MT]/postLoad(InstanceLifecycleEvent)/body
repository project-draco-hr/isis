{
  if (LOG.isDebugEnabled()) {
    LOG.debug(logString(Phase.POST,event));
  }
  if (isSuspended()) {
    if (LOG.isDebugEnabled()) {
      LOG.debug(" [currently suspended - ignoring]");
    }
    return;
  }
  final PersistenceCapable pojo=persistenceCapableFor(event);
  postLoadProcessingFor(pojo);
}
