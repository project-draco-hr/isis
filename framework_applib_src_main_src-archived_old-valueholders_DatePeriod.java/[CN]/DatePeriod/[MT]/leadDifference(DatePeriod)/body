{
  if (!this.overlaps(arg)) {
    throw new IllegalArgumentException("No overlap");
  }
 else {
    if (this.startsBefore(arg)) {
      getEndDate().setValue(arg.getStart());
      getEndDate().add(0,0,-1);
    }
 else {
      getEndDate().setValue(getStart());
      getEndDate().add(0,0,-1);
      getStart().setValue(arg.getStart());
    }
  }
}
