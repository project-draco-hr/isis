{
  if (debugPane != null) {
    debugPane.write(writer);
  }
 else {
    writer.println();
    writer.println("<div id=\"body\">");
    navigation.write(writer);
    if (crumbs != null) {
      crumbs.write(writer);
    }
    viewPane.write(writer);
    writer.println();
    writer.println("</div>");
  }
}
