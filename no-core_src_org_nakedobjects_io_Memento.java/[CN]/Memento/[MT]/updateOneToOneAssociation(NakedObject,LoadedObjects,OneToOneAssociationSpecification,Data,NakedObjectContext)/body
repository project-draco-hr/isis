{
  if (fieldData == null) {
    field.initData(object,null);
  }
 else {
    NakedObject ref=recreateObject2(loadedObjects,fieldData,context);
    if (field.get(object) != ref) {
      LOG.debug("  association " + field + " changed to "+ ref.getOid());
      field.initData(object,ref);
    }
  }
}
