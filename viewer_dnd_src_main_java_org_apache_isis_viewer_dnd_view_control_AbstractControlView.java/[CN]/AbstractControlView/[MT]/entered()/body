{
  final View target=getParent();
  final Consent consent=action.disabled(target);
  String actionText=action.getName(target) + " - " + (consent.isVetoed() ? consent.getReason() : action.getDescription(target));
  getFeedbackManager().setAction(actionText);
  isOver=true;
  isPressed=false;
  markDamaged();
}
