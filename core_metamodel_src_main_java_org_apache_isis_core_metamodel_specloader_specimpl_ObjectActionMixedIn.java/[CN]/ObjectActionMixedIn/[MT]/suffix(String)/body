{
  if (singularName.endsWith("_")) {
    if (Objects.equal(singularName,"_")) {
      return singularName;
    }
    return singularName;
  }
  final int indexOfUnderscore=singularName.lastIndexOf('_');
  if (indexOfUnderscore == -1) {
    return singularName;
  }
  return singularName.substring(indexOfUnderscore + 1);
}
