{
  for (int i=0; i < actions.length; i++) {
    if (actions[i].getActions().length > 0) {
      final ObjectAction action=findAction(actions[i].getActions(),methodName);
      if (action != null) {
        return action;
      }
    }
 else {
      if (actions[i].getId().equals(methodName)) {
        return actions[i];
      }
    }
  }
  return null;
}
