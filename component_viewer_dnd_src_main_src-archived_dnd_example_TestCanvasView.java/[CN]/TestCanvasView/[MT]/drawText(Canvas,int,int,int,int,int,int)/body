{
  canvas.drawLine(x,baseline,x + 200 - 1,baseline,gray);
  canvas.drawLine(x,baseline - midpoint,x + 200 - 1,baseline - midpoint,red);
  canvas.drawLine(x,baseline - ascent,x + 200 - 1,baseline - ascent,lightGray);
  canvas.drawLine(x,baseline + descent,x + 200 - 1,baseline + descent,yellow);
  canvas.drawText("12345 abcdefghijk ABCDEFG",x,baseline,black,Toolkit.getText(ColorsAndFonts.TEXT_NORMAL));
}
