{
  if (cachedToString == null) {
    final StringBuffer str=new StringBuffer();
    str.append("ResolveState [name=");
    str.append(name);
    str.append(",code=");
    str.append(code);
    if (endState != null) {
      str.append(",endstate=");
      str.append(endState.name());
    }
    str.append("]");
    cachedToString=str.toString();
  }
  return cachedToString;
}
