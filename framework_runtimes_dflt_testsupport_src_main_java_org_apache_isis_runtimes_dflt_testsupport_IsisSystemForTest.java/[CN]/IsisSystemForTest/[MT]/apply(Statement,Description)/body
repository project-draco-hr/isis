{
  return new Statement(){
    @Override public void evaluate() throws Throwable {
      setUpSystem();
      try {
        base.evaluate();
        tearDownSystem();
      }
 catch (      Throwable ex) {
        try {
          tearDownSystem();
        }
 catch (        Exception ex2) {
        }
        throw ex;
      }
    }
  }
;
}
