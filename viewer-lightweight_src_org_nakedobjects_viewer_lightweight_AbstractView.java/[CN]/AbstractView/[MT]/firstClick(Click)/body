{
  if (border != null) {
    border.firstClick(this,click);
  }
  for (int i=0; i < controls.size(); i++) {
    Control control=(Control)controls.elementAt(i);
    Location at=click.getLocation();
    if (control.getBounds().contains(at)) {
      control.invoke(getWorkspace(),this,at);
      return;
    }
  }
}
