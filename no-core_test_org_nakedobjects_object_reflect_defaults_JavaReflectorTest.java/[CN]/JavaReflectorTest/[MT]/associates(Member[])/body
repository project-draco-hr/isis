{
  Vector v=new Vector();
  for (int i=0; i < fields.length; i++) {
    Member member=fields[i];
    if (member instanceof OneToOneAssociation && ((OneToOneAssociation)member).hasAddMethod()) {
      v.addElement(member.getName());
    }
  }
  String[] names=new String[v.size()];
  v.copyInto(names);
  return names;
}
