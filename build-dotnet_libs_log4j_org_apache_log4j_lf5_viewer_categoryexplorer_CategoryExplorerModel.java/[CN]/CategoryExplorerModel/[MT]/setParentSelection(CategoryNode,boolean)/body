{
  TreeNode[] nodes=getPathToRoot(node);
  int len=nodes.length;
  CategoryNode parent;
  for (int i=1; i < len; i++) {
    parent=(CategoryNode)nodes[i];
    if (parent.isSelected() != selected) {
      parent.setSelected(selected);
      nodeChanged(parent);
    }
  }
  notifyActionListeners();
}
